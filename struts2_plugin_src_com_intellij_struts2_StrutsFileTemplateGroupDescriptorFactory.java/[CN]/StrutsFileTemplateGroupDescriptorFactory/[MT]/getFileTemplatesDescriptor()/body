{
  final FileTemplateGroupDescriptor group=new FileTemplateGroupDescriptor(StrutsBundle.message("struts2"),StrutsIcons.ACTION);
  group.addTemplate(new FileTemplateDescriptor(STRUTS_2_0_XML,StrutsIcons.STRUTS_CONFIG_FILE_ICON));
  group.addTemplate(new FileTemplateDescriptor(STRUTS_2_1_XML,StrutsIcons.STRUTS_CONFIG_FILE_ICON));
  group.addTemplate(new FileTemplateDescriptor("validator.xml",StrutsIcons.VALIDATION_CONFIG_FILE_ICON));
  return group;
}
