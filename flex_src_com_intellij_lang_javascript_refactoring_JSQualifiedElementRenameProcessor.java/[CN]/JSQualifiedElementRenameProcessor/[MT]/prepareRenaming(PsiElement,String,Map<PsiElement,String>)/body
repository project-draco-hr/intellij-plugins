{
  if (element instanceof JSClass) {
    JSFunction constructor=((JSClass)element).getConstructor();
    if (constructor != null) {
      allRenames.put(constructor,newName);
    }
  }
  if (element instanceof JSQualifiedNamedElement) {
    PsiFile containingFile=element.getContainingFile();
    if ((!(containingFile instanceof JSFileImpl) || JSFileImpl.findMainDeclaredElement((JSFileImpl)containingFile) == element) && JSInheritedLanguagesHelper.shouldRenameFileWithClass(element)) {
      allRenames.put(containingFile,newName + "." + containingFile.getVirtualFile().getExtension());
    }
  }
}
