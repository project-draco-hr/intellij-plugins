{
  final CompilerModuleExtension compilerModuleExtension=(CompilerModuleExtension)rootModel.getModuleExtension(CompilerModuleExtension.class).getModifiableModel(true);
  Disposer.register(rootModel.getModule(),compilerModuleExtension);
  compilerModuleExtension.inheritCompilerOutputPath(false);
  final String outputFolderUrl=VfsUtil.pathToUrl(getAbsolutePathWithLinksHandled(flashBuilderProject,flashBuilderProject.getOutputFolderPath()));
  compilerModuleExtension.setCompilerOutputPath(outputFolderUrl);
  compilerModuleExtension.setCompilerOutputPathForTests(outputFolderUrl);
  compilerModuleExtension.commit();
}
