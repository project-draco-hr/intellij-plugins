{
  final FileTemplateGroupDescriptor group=new FileTemplateGroupDescriptor("Struts 2",StrutsIcons.ACTION);
  group.addTemplate(new FileTemplateDescriptor(StrutsConstants.STRUTS_DEFAULT_FILENAME,StrutsIcons.STRUTS_CONFIG_FILE_ICON));
