{
  LineHits prev=null;
  for (  LineHits cur : report) {
    if (prev != null && prev.getLineNumber() > cur.getLineNumber()) {
      LineHits[] array=report.toArray(new LineHits[report.size()]);
      Arrays.sort(array);
      return ImmutableList.of(array);
    }
    prev=cur;
  }
  return report;
}
