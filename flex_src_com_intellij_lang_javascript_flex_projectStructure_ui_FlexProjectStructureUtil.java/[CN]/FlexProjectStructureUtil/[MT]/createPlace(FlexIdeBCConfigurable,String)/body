{
  Place place=new Place().putPath(ProjectStructureConfigurable.CATEGORY,ModuleStructureConfigurable.getInstance(configurable.getModule().getProject())).putPath(MasterDetailsComponent.TREE_OBJECT,configurable.getEditableObject());
  if (tabName != null) {
    place.putPath(CompositeConfigurable.TAB_NAME,tabName);
  }
  return place;
}
