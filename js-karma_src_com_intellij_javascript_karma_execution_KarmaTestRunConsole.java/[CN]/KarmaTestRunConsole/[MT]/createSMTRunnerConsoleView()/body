{
  KarmaRunConfiguration runConfiguration=(KarmaRunConfiguration)myEnvironment.getRunProfile();
  TestConsoleProperties testConsoleProperties=new SMTRunnerConsoleProperties(new RuntimeConfigurationProducer.DelegatingRuntimeConfiguration<KarmaRunConfiguration>(runConfiguration),FRAMEWORK_NAME,myExecutor);
  testConsoleProperties.setIfUndefined(TestConsoleProperties.HIDE_PASSED_TESTS,false);
  KarmaProxyPrinterProvider printerProvider=new KarmaProxyPrinterProvider(myKarmaServer.getKarmaConfig());
  SMTRunnerConsoleView consoleView=SMTestRunnerConnectionUtil.createConsoleWithCustomLocator(FRAMEWORK_NAME,testConsoleProperties,myEnvironment.getRunnerSettings(),myEnvironment.getConfigurationSettings(),new KarmaTestLocationProvider(),true,printerProvider);
  printerProvider.setConsoleView(consoleView);
  return consoleView;
}
