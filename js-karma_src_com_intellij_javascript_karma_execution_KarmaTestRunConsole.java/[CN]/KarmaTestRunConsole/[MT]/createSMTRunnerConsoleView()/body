{
  KarmaRunConfiguration runConfiguration=(KarmaRunConfiguration)myEnvironment.getRunProfile();
  TestConsoleProperties testConsoleProperties=new SMTRunnerConsoleProperties(new RuntimeConfigurationProducer.DelegatingRuntimeConfiguration<KarmaRunConfiguration>(runConfiguration),FRAMEWORK_NAME,myExecutor);
  testConsoleProperties.setUsePredefinedMessageFilter(false);
  testConsoleProperties.setIfUndefined(TestConsoleProperties.HIDE_PASSED_TESTS,false);
  Project project=testConsoleProperties.getProject();
  return SMTestRunnerConnectionUtil.createConsoleWithCustomLocator(FRAMEWORK_NAME,testConsoleProperties,myEnvironment.getRunnerSettings(),myEnvironment.getConfigurationSettings(),new KarmaTestLocationProvider(),true,new KarmaTestProxyFilterProvider(project,myKarmaServer));
}
