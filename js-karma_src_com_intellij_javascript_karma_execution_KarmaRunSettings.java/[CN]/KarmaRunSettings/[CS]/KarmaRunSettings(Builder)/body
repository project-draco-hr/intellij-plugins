{
  myConfigPath=FileUtil.toSystemDependentName(builder.myConfigPath);
  myKarmaPackageDir=builder.myKarmaPackageDir != null ? FileUtil.toSystemDependentName(builder.myKarmaPackageDir) : null;
  myBrowsers=builder.myBrowsers;
