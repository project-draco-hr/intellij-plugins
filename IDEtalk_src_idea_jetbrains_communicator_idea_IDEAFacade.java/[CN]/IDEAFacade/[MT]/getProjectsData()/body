{
  final ProjectsData result=new ProjectsData();
  ApplicationManager.getApplication().runReadAction(new Runnable(){
    public void run(){
      try {
        new ProjectsDataFiller(result).fillProjectsData();
      }
 catch (      Throwable e) {
        LOG.info(e,e);
      }
    }
  }
);
  return result;
}
