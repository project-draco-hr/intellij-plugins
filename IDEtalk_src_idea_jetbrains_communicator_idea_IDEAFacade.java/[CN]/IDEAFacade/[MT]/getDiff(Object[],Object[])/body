{
  List<MyChangeAdapter> result=new ArrayList<MyChangeAdapter>();
  Diff.Change change;
  try {
    change=Diff.buildChanges(src,dest);
  }
 catch (  Exception e) {
    LOG.warn(e);
    return new Change[0];
  }
  while (change != null) {
    result.add(new MyChangeAdapter(change));
    change=change.link;
  }
  return result.toArray(new Change[result.size()]);
}
