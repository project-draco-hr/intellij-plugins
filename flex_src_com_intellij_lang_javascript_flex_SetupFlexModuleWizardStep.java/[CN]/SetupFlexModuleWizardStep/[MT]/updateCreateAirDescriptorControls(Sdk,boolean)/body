{
  myCreateAirDescriptorCheckBox.setEnabled(myApplicationOutputTypeRadioButton.isSelected() && srcRootExists && sdk != null && (sdk.getSdkType() instanceof AirSdkType || sdk.getSdkType() instanceof AirMobileSdkType));
  myAirDescriptorFileNameTextField.setEnabled(myCreateAirDescriptorCheckBox.isEnabled() && myCreateAirDescriptorCheckBox.isSelected());
}
