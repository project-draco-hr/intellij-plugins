{
  String extension=FileUtil.getExtension(selectedFile.getName());
  if (!extension.isEmpty()) {
    return selectedFile;
  }
  if (selectedFile.exists()) {
    return selectedFile;
  }
  if (selectedFileFilter == JSTD_CONFIG_FILE_FILTER) {
    return new File(selectedFile.getAbsolutePath() + JSTD_EXT);
  }
  return selectedFile;
}
