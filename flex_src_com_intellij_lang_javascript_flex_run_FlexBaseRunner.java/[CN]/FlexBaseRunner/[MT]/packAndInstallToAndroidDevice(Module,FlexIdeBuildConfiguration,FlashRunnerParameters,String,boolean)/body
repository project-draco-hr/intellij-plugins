{
  final Project project=module.getProject();
  final Sdk sdk=bc.getSdk();
  final String apkPath=bc.getOutputFolder() + "/" + bc.getAndroidPackagingOptions().getPackageFileName()+ ".apk";
  final String adtVersion;
  return (adtVersion=AirPackageUtil.getAdtVersion(project,sdk)) != null && AirPackageUtil.checkAdtVersion(module,bc,adtVersion) && AirPackageUtil.checkAirRuntimeOnDevice(project,sdk,adtVersion) && (!bc.getAndroidPackagingOptions().getSigningOptions().isUseTempCertificate() || AirPackageUtil.ensureCertificateExists(project,sdk)) && AirPackageUtil.packageApk(project,bc,runnerParameters,isDebug) && AirPackageUtil.installApk(project,sdk,apkPath,applicationId);
}
