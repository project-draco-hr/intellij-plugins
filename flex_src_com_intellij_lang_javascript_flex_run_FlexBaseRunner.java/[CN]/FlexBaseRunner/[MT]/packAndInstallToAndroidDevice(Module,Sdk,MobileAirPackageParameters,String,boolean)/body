{
  final Project project=module.getProject();
  final String apkPath=packageParameters.INSTALLER_FILE_LOCATION + "/" + packageParameters.INSTALLER_FILE_NAME;
  final String adtVersion;
  return (adtVersion=MobileAirUtil.getAdtVersion(project,flexSdk)) != null && MobileAirUtil.checkAdtVersion(module,flexSdk,adtVersion) && MobileAirUtil.checkAirRuntimeOnDevice(project,flexSdk,adtVersion) && MobileAirUtil.ensureCertificateExists(project,flexSdk) && MobileAirUtil.packageApk(project,packageParameters) && MobileAirUtil.installApk(project,flexSdk,apkPath,applicationId);
}
