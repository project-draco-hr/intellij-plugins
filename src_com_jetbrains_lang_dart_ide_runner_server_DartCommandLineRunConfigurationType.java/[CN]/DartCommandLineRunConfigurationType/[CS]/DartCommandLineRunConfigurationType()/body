{
  super("DartCommandLineRunConfigurationType",DartBundle.message("runner.command.line.configuration.name"),DartBundle.message("runner.command.line.configuration.name"),icons.DartIcons.Dart_16);
  addFactory(new ConfigurationFactory(this){
    @Override public RunConfiguration createTemplateConfiguration(    Project project){
      return new DartCommandLineRunConfiguration("Dart",project,DartCommandLineRunConfigurationType.this);
    }
  }
);
}
