{
  if (project != null) {
    final VirtualFile pubspec=PubspecYamlUtil.findPubspecYamlFile(project,contextFile);
    if (pubspec != null) {
      final VirtualFile parent=pubspec.getParent();
      if (parent != null) {
        return parent.getPath();
      }
    }
  }
  return contextFile.getParent().getPath();
}
