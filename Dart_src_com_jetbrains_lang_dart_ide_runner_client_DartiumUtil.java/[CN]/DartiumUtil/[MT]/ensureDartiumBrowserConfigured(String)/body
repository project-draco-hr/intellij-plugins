{
  final WebBrowser browser=getDartiumBrowser();
  if (browser == null) {
    WebBrowserManager.getInstance().addBrowser(DARTIUM_ID,BrowserFamily.CHROME,DARTIUM_NAME,dartiumPath,true,BrowserFamily.CHROME.createBrowserSpecificSettings());
  }
 else   if (!dartiumPath.equals(browser.getPath())) {
    WebBrowserManager.getInstance().setBrowserPath(browser,dartiumPath,true);
  }
}
