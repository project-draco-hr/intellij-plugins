{
  ChromeDebugProcess debugProcess=super.createDebugProcess(session,browser,fileFinder,initialUrl,executionResult,usePreliminaryPage);
  debugProcess.setProcessBreakpointConditionsAtIdeSide(true);
  debugProcess.setBreakpointLanguageHint(new PairFunction<XLineBreakpoint<?>,Location,String>(){
    @Override public String fun(    XLineBreakpoint<?> breakpoint,    Location location){
      String result=StringUtil.endsWithIgnoreCase(breakpoint == null ? location.getUrl().getPath() : breakpoint.getFileUrl(),".dart") ? "dart" : null;
      if (LOG.isDebugEnabled()) {
        LOG.debug(breakpoint + ", " + location.getUrl()+ " "+ result);
      }
      return result;
    }
  }
);
  return debugProcess;
}
