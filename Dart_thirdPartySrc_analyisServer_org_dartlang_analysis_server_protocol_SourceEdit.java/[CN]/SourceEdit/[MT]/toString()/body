{
  StringBuilder builder=new StringBuilder();
  builder.append("[");
  builder.append("offset=");
  builder.append(offset + ", ");
  builder.append("length=");
  builder.append(length + ", ");
  builder.append("replacement=");
  builder.append(replacement + ", ");
  builder.append("id=");
  builder.append(id);
  builder.append("]");
  return builder.toString();
}
