{
  if (myFieldRef == null) {
    navigatable.setSourcePosition(null);
    return;
  }
  myDebugProcess.getVmServiceWrapper().getObject(myIsolateId,myFieldRef.getId(),new GetObjectConsumer(){
    @Override public void received(    final Obj field){
      reportSourcePosition(navigatable,((Field)field).getLocation());
    }
    @Override public void received(    final Sentinel sentinel){
      navigatable.setSourcePosition(null);
    }
    @Override public void onError(    final RPCError error){
      navigatable.setSourcePosition(null);
    }
  }
);
}
