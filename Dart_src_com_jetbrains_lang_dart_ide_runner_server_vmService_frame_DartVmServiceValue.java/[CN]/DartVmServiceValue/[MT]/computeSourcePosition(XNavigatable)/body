{
  if (myLocalVarSourceLocation != null) {
    reportSourcePosition(myDebugProcess,navigatable,myIsolateId,myLocalVarSourceLocation.myScriptRef,myLocalVarSourceLocation.myTokenPos);
  }
 else   if (myFieldRef != null) {
    doComputeSourcePosition(myDebugProcess,navigatable,myIsolateId,myFieldRef);
  }
 else {
    navigatable.setSourcePosition(null);
  }
}
