{
  if (methodName == null) {
    return null;
  }
  final PsiClass actionClass=getActionClass().getValue();
  if (actionClass == null) {
    return null;
  }
  final List<PsiMethod> actionMethods=ActionUtil.findActionMethods(actionClass,methodName);
  return actionMethods.size() == 1 ? actionMethods.get(0) : null;
}
