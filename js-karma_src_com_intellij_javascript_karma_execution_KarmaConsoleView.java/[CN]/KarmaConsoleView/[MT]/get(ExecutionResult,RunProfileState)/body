{
  ExecutionConsole console=result.getExecutionConsole();
  if (console instanceof KarmaConsoleView) {
    return (KarmaConsoleView)console;
  }
  Class consoleClass=console != null ? console.getClass() : null;
  throw new AssertionError("Cannot cast " + consoleClass + " to "+ KarmaConsoleView.class.getSimpleName()+ ", RunProfileState: "+ state.getClass().getName());
}
