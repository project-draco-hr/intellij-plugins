{
  super(new OgnlLexer());
  final StringLiteralLexer stringLiteralLexer=new StringLiteralLexer('\"',OgnlTokenTypes.STRING_LITERAL);
  registerSelfStoppingLayer(stringLiteralLexer,new IElementType[]{OgnlTokenTypes.STRING_LITERAL},IElementType.EMPTY_ARRAY);
  final StringLiteralLexer characterLiteralLexer=new StringLiteralLexer('\'',OgnlTokenTypes.CHARACTER_LITERAL);
  registerSelfStoppingLayer(characterLiteralLexer,new IElementType[]{OgnlTokenTypes.CHARACTER_LITERAL},IElementType.EMPTY_ARRAY);
}
