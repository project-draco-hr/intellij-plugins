{
  SwfUtil.header(totalLength,out,buffer);
  out.write(B_ABC);
  encodeLongTagHeader(TagTypes.DefineBitsJPEG2,2 + dataLength);
  buffer.putShort((short)1);
  write(out);
  FileUtil.copy(in,out);
  encodeTagHeader(TagTypes.SymbolClass,SYMBOL_CLASS_TAG_LENGTH);
  buffer.putShort((short)1);
  buffer.putShort((short)1);
  writeSwfString('B');
  write(out);
  SwfUtil.footer(out);
}
