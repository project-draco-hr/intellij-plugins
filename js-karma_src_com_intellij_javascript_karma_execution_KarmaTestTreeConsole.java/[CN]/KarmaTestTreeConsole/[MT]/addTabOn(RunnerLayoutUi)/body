{
  Content consoleContent=ui.createContent(ExecutionConsole.CONSOLE_CONTENT_ID,getComponent(),"Tests Result",AllIcons.Debugger.Console,getPreferredFocusableComponent());
  consoleContent.setBusyObject(new BusyObject(){
    @Override public ActionCallback getReady(    @NotNull Object requestor){
      return myProcessHandlerInfo.myOsProcessHandlerAsyncResult;
    }
  }
);
  consoleContent.setCloseable(false);
  ui.addContent(consoleContent,0,PlaceInGrid.bottom,false);
}
