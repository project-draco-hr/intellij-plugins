{
  try {
    for (    String targetPath : targetPaths) {
      FileUtil.copyContent(file,new File(targetPath));
      outputConsumer.registerOutputFile(targetPath,Collections.singletonList(file.getPath()));
    }
  }
 catch (  IOException e) {
    context.processMessage(new CompilerMessage(BUILDER_NAME,BuildMessage.Kind.ERROR,e.getMessage(),FileUtil.toSystemIndependentName(file.getPath())));
  }
}
