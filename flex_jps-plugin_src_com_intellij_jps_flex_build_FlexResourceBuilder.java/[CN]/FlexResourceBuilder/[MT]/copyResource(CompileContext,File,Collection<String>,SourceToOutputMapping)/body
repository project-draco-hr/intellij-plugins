{
  try {
    for (    String targetPath : targetPaths) {
      FileUtil.copyContent(file,new File(targetPath));
    }
    try {
      sourceToOutputMapping.setOutputs(file.getPath(),targetPaths);
    }
 catch (    Exception e) {
      context.processMessage(new CompilerMessage(BUILDER_NAME,e));
    }
  }
 catch (  IOException e) {
    context.processMessage(new CompilerMessage("Flex Resource Compiler",BuildMessage.Kind.ERROR,e.getMessage(),FileUtil.toSystemIndependentName(file.getPath())));
  }
}
