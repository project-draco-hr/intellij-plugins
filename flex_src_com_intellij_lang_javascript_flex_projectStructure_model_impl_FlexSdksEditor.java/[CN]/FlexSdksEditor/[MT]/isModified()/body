{
  List<Library> originalSdkLibraries=ContainerUtil.filter(ApplicationLibraryTable.getApplicationTable().getLibraries(),new Condition<Library>(){
    @Override public boolean value(    Library library){
      return FlexSdk.isFlexSdk(library);
    }
  }
);
  return !Arrays.asList(myConfigEditor.getSdksLibraries()).equals(originalSdkLibraries);
}
