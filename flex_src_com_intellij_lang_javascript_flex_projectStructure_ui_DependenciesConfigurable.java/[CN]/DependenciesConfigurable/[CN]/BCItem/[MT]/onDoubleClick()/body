{
  if (configurable != null) {
    Project project=configurable.getModule().getProject();
    Place place=new Place().putPath(ProjectStructureConfigurable.CATEGORY,ModuleStructureConfigurable.getInstance(project)).putPath(MasterDetailsComponent.TREE_OBJECT,configurable.getEditableObject());
    ProjectStructureConfigurable.getInstance(project).navigateTo(place,true);
  }
}
