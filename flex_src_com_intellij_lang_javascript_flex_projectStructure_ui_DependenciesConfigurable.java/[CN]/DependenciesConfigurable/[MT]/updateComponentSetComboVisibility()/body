{
  final SdkEntry sdkEntry=mySdkPanel.getCurrentSdk();
  final boolean visible=sdkEntry != null && StringUtil.compareVersionNumbers(sdkEntry.getFlexVersion(),"4") >= 0 && !myNature.isMobilePlatform() && !myNature.pureAS;
  myComponentSetLabel.setVisible(visible);
  myComponentSetCombo.setVisible(visible);
}
