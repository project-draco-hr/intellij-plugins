{
  if (willBeRestarted) {
    context.addMessage(CompilerMessageCategory.WARNING,FlexBundle.message("fcsh.out.of.memory.and.restarted",CommonBundle.settingsActionPath()),null,-1,-1);
  }
 else {
    context.addMessage(CompilerMessageCategory.ERROR,FlexCommonBundle.message("increase.flex.compiler.heap",CommonBundle.settingsActionPath()),null,-1,-1);
  }
}
