{
  try {
    int startOffset=entireLength - line.length();
    Result result=parseLine(line,myProject,myLibraryRootPath);
    return result == null ? null : new Result(startOffset + result.highlightStartOffset,startOffset + result.highlightEndOffset,result.hyperlinkInfo);
  }
 catch (  ProcessCanceledException e) {
    return null;
  }
}
