{
  final File file=new File(cacheDir,NAME_PREFIX + "librarySets");
  try {
    return new PersistentHashMap<String,SortResult>(file,new EnumeratorStringDescriptor(),new LibrarySetDataExternalizer());
  }
 catch (  IOException e) {
    LogMessageUtil.LOG.info(e);
    clearCache(file);
    return new PersistentHashMap<String,SortResult>(file,new EnumeratorStringDescriptor(),new LibrarySetDataExternalizer());
  }
}
