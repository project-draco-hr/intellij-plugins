{
  builder.append(function.getName());
  builder.append('(');
  builder.append(DartPresentableUtil.getPresentableParameterList(function));
  builder.append(')');
  if (returnType != null) {
    builder.append(' ');
    builder.append(RIGHT_ARROW);
    builder.append(' ');
    builder.append(DartPresentableUtil.buildTypeText(null,returnType,null));
  }
}
