{
  registrar.registerReferenceProvider(xmlAttributeValue().withLocalName("name").withSuperParent(2,withDom(domElement(Result.class))),new StaticStringValuesReferenceProvider("error","input","login","success"));
  registrar.registerReferenceProvider(xmlTag().and(withDom(domElement(Result.class))).withText(string().contains("${")),new ResultActionPropertyReferenceProvider());
}
