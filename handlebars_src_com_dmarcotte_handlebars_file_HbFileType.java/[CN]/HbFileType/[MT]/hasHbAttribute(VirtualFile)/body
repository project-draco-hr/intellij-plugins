{
  if (!(file instanceof NewVirtualFile) || !file.isValid()) {
    return false;
  }
  try {
    DataInputStream inputStream=HANDLEBARS_ATTRIBUTE.readAttribute(file);
    try {
      return inputStream != null && inputStream.readBoolean();
    }
  finally {
      if (inputStream != null) {
        inputStream.close();
      }
    }
  }
 catch (  IOException e) {
    return false;
  }
}
