{
  if (!(file instanceof NewVirtualFile) || !file.isValid()) {
    return;
  }
  DataOutputStream outputStream=HANDLEBARS_ATTRIBUTE.writeAttribute(file);
  try {
    try {
      outputStream.writeBoolean(value);
    }
  finally {
      outputStream.close();
    }
  }
 catch (  IOException e) {
  }
}
