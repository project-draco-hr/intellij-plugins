{
  myContext=context;
  myExtension=target.getExtension();
  myModule=myExtension.getModule();
  myModuleNameMessagePrefix="[" + myModule.getName() + "] ";
  progressMessage("Building bundle for the module");
  try {
    prepare();
    doBuild();
  }
 catch (  OsmorcBuildException e) {
    processException(e);
  }
}
