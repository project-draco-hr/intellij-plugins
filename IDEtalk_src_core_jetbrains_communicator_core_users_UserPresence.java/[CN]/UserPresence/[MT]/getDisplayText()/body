{
switch (myPresenceMode) {
case UNAVAILABLE:
    return "";
case AVAILABLE:
  return StringUtil.getMsg("user.presence.online");
case DND:
return StringUtil.getMsg("user.presence.dnd");
case AWAY:
return StringUtil.getMsg("user.presence.away");
case EXTENDED_AWAY:
return StringUtil.getMsg("user.presence.extended_away");
}
return "";
}
