{
  if (getElements().length == 1) {
    return FlexBundle.message("element.to.be.moved.to",JSBundle.message(JSNamedElementKind.kind((JSNamedElement)getElements()[0]).humanReadableKey()),JSFormatUtil.formatPackage(myTargetPackage));
  }
 else {
    return FlexBundle.message("elements.to.be.moved.to",JSFormatUtil.formatPackage(myTargetPackage));
  }
}
