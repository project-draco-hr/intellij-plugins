{
  if (!super.canFinish()) {
    return false;
  }
  if (isSuperclassFieldEnabled()) {
    if (!JSUtils.isValidClassName(getSuperclassFqn(),true)) {
      return false;
    }
    if (!(JSClassResolver.findClassByQName(getSuperclassFqn(),getSuperclassScope()) instanceof JSClass)) {
      return false;
    }
  }
  return true;
}
