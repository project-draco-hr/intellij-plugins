{
  if (StringUtil.isNotEmpty(repositoryRootPath) && StringUtil.isNotEmpty(groupId) && StringUtil.isNotEmpty(artifactId)&& StringUtil.isNotEmpty(version)) {
    myFlexCompilerClasspath.add(repositoryRootPath + '/' + groupId.replace('.','/')+ '/'+ artifactId+ '/'+ version+ '/'+ artifactId+ '-'+ version+ ".jar");
  }
}
