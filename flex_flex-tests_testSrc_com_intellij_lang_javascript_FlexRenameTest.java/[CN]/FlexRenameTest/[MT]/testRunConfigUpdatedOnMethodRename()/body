{
  final RunManagerEx runManager=RunManagerEx.getInstanceEx(myProject);
  JSTestUtils.createFlexUnitRunConfig(runManager,"SomeTest.testSomething()",myModule,FlexUnitRunnerParameters.Scope.Method,"","SomeTest","testSomething",true);
  doTest("testAnotherThing","as");
  JSTestUtils.checkFlexUnitRunConfig(runManager,myModule,"SomeTest.testAnotherThing()","","SomeTest","testAnotherThing");
}
