{
  final ProjectWizardStepFactory stepFactory=ProjectWizardStepFactory.getInstance();
  if (PlatformUtils.isFlexIde()) {
    return new ModuleWizardStep[]{new SelectDirWithFlashBuilderProjectsStep(context),new SelectFlashBuilderImportedProjectsStep(context),stepFactory.createNameAndLocationStep(context)};
  }
 else {
    return new ModuleWizardStep[]{new SelectDirWithFlashBuilderProjectsStep(context),new SelectFlashBuilderImportedProjectsStep(context),stepFactory.createProjectJdkStep(context),stepFactory.createNameAndLocationStep(context)};
  }
}
