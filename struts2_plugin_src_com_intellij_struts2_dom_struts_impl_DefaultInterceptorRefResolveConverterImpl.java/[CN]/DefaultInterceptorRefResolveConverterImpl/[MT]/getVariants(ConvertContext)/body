{
  final List<InterceptorStack> results=new SmartList<InterceptorStack>();
  final Processor<StrutsPackage> processor=new Processor<StrutsPackage>(){
    @Override public boolean process(    final StrutsPackage strutsPackage){
      results.addAll(strutsPackage.getInterceptorStacks());
      return false;
    }
  }
;
  final StrutsPackageHierarchyWalker walker=new StrutsPackageHierarchyWalker(getCurrentStrutsPackage(context),processor);
  walker.walkUp();
  return results;
}
