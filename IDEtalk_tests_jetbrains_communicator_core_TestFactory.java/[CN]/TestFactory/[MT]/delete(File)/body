{
  if (file.isDirectory()) {
    File[] files=file.listFiles();
    for (int i=0; i < files.length; i++) {
      delete(files[i]);
    }
  }
  int i=0;
  file.delete();
  while (file.exists() && i < 5) {
    Thread.sleep(30);
    file.delete();
    i++;
  }
  Assert.assertFalse("Should be cleanuped:" + file,file.exists());
}
