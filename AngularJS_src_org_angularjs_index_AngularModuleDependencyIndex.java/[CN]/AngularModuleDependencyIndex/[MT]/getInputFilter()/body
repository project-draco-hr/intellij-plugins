{
  return new FileBasedIndex.InputFilter(){
    @Override public boolean acceptInput(    @NotNull VirtualFile file){
      if (file.getFileType() instanceof LanguageFileType) {
        return ((LanguageFileType)file.getFileType()).getLanguage().isKindOf(JavascriptLanguage.INSTANCE) && !(file.getFileSystem() instanceof JarFileSystem);
      }
      return false;
    }
  }
;
}
