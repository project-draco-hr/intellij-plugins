{
  if (element == null)   return null;
  final List<PsiElement> result=new ArrayList<PsiElement>();
  while (element != null) {
    result.add(element);
    if (aClass.isInstance(element)) {
      return result;
    }
    if (element instanceof PsiFile)     return null;
    element=element.getParent();
  }
  return null;
}
