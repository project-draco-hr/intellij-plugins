{
  if (CSS_ELEMENT_PATTERN.accepts(psiElement)) {
    final TextRange range=new TextRange(1,psiElement.getTextLength() - 1);
    multiHostRegistrar.startInjecting(CssSupportLoader.CSS_FILE_TYPE.getLanguage()).addPlace("inline.style {","}",(PsiLanguageInjectionHost)psiElement,range).doneInjecting();
  }
}
