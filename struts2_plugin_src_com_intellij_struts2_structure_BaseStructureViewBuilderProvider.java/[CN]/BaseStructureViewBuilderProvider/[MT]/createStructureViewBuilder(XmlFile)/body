{
  final DomFileElement fileElement=getFileElement(xmlFile);
  if (fileElement == null) {
    return null;
  }
  return new DomStructureViewBuilder(xmlFile,ALWAYS_SHOW){
    @NotNull @Override public StructureViewModel createStructureViewModel(){
      return new StructureViewTreeModel(xmlFile,getAlwaysPlus(),getAlwaysLeaf(),ALWAYS_SHOW);
    }
  }
;
}
