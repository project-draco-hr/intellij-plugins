{
  InputStream inputStream=JstdIcons.class.getResourceAsStream(resourceName);
  try {
    if (inputStream == null) {
      throw new RuntimeException("Can't find resource by name '" + resourceName + "'");
    }
    return ImageIO.read(inputStream);
  }
  finally {
    if (inputStream != null) {
      inputStream.close();
    }
  }
}
