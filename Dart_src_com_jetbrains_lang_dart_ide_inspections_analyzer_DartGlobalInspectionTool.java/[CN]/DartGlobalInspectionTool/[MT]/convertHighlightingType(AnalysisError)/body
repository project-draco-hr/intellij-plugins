{
switch (analysisError.getErrorCode().getErrorSeverity()) {
case NONE:
    return ProblemHighlightType.INFORMATION;
case INFO:
  return ProblemHighlightType.INFORMATION;
case WARNING:
return ProblemHighlightType.GENERIC_ERROR_OR_WARNING;
case ERROR:
return ProblemHighlightType.ERROR;
}
return ProblemHighlightType.INFORMATION;
}
