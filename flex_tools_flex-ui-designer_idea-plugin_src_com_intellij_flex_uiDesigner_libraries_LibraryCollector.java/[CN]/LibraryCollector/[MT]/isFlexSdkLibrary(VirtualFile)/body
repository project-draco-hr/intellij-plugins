{
  final String name=file.getName();
  if (name.startsWith(FlexLibsNames.FRAMEWORK)) {
    return JSResolveUtil.findClassByQName("FrameworkClasses",GlobalSearchScope.fileScope(project,file)) != null;
  }
 else   if (name.startsWith(FlexLibsNames.SPARK)) {
    return JSResolveUtil.findClassByQName("SparkClasses",GlobalSearchScope.fileScope(project,file)) != null;
  }
  return false;
}
