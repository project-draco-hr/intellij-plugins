{
  if (jarFile.getFileSystem() instanceof JarFileSystem) {
    VirtualFile file=JarFileSystem.getInstance().getVirtualFileForJar(jarFile);
    if (file != null && !file.isDirectory() && file.getName().endsWith(".swc") && !isGlobalLibrary(file.getName()) && isSwfAndCatalogExists(jarFile) && uniqueGuard.add(file)) {
      return file;
    }
  }
  return null;
}
