{
  final List<Library> list=isFromFlexSdk ? sdkLibraries : externalLibraries;
  final String artifactId;
  if (isFromFlexSdk && flexmojosFlexSdkRootPath != null) {
    final String fileName=file.getName();
    artifactId=fileName.substring(0,fileName.length() - (1 + flexSdkVersion.length() + DOT_SWC.length()));
  }
 else {
    artifactId=file.getNameWithoutExtension();
  }
  list.add(libraryManager.createOriginalLibrary(jarFile,artifactId,initializer));
}
