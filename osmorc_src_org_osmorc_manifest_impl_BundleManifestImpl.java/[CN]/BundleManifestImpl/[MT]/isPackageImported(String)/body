{
  Object headerValue=getHeaderValue(IMPORT_PACKAGE);
  if (headerValue != null) {
    @SuppressWarnings("unchecked") List<String> packages=(List<String>)headerValue;
    for (    String imported : packages) {
      if (PsiNameHelper.isSubpackageOf(packageName,imported)) {
        return true;
      }
    }
  }
  return false;
}
