{
  Header header=fragmentBundle.getManifestFile().getHeaderByName(FRAGMENT_HOST);
  if (header == null) {
    return false;
  }
  Clause[] clauses=header.getClauses();
  if (clauses.length != 1) {
    return false;
  }
  Clause clause=clauses[0];
  String hostSpec=clause.getClauseText();
  return isRequiredBundle(hostSpec);
}
