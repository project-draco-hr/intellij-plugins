{
  try {
    socket=serverSocket.accept();
    serverSocket.close();
    setOut(socket.getOutputStream());
    serverSocket=null;
  }
 catch (  IOException e) {
    if (serverSocket != null) {
      LOG.error(e);
      IOUtil.close(socket);
      socket=null;
      clientSocketNotAccepted();
    }
    return;
  }
  clientSocketAccepted();
}
