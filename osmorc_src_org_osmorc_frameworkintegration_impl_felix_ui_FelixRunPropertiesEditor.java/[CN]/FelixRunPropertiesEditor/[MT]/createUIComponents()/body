{
  _presentationModel=new PresentationModel<FelixRunProperties>(new FelixRunProperties(new HashMap<String,String>()));
  _debugCheckbox=BasicComponentFactory.createCheckBox(_presentationModel.getModel(FelixRunProperties.DEBUG_MODE),"");
  _systemPackages=BasicComponentFactory.createTextField(_presentationModel.getModel(FelixRunProperties.SYSTEM_PACKAGES));
  _bootDelegation=BasicComponentFactory.createTextField(_presentationModel.getModel(FelixRunProperties.BOOT_DELEGATION));
}
