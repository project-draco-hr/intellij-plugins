{
  final String path=myRunnerParameters.getHtmlOrSwfFilePath();
  return myRunnerParameters.getRunMode() == FlexRunnerParameters.RunMode.MainClass ? StringUtil.getShortName(myRunnerParameters.getMainClassName()) : myRunnerParameters.getRunMode() == FlexRunnerParameters.RunMode.HtmlOrSwfFile ? path.substring(path.lastIndexOf('/') + 1) : "unnamed";
}
