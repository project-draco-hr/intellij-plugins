{
  final XmlResponseProvider providers[]=new XmlResponseProvider[]{new GetVFileContentsProvider(ideFacade,userModel),new GetProjectsDataProvider(ideFacade,userModel),new CodePointerEventProvider(getBroadcaster()),new TextMessageEventProvider(getBroadcaster()),new BecomeAvailableXmlMessage()};
  for (int i=0; i < providers.length; i++) {
    Pico.getInstance().registerComponentInstance(providers[i]);
  }
  disposeOnTearDown(new Disposable(){
    public void dispose(){
      for (int i=0; i < providers.length; i++) {
        Pico.getInstance().unregisterComponentByInstance(providers[i]);
      }
    }
  }
);
}
