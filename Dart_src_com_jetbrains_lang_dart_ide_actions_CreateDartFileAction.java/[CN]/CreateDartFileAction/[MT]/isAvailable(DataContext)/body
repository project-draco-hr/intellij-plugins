{
  final Module module=LangDataKeys.MODULE.getData(dataContext);
  return super.isAvailable(dataContext) && module != null && !StringUtil.isEmptyOrSpaces(DartSettings.getSettings().getSdkPath()) && (ModuleType.get(module) instanceof WebModuleTypeBase || FileTypeIndex.containsFileOfType(DartFileType.INSTANCE,GlobalSearchScope.moduleScope(module)));
}
