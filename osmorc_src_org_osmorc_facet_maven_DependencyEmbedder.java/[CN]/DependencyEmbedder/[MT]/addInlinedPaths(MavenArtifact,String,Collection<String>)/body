{
  File path=dependency.getFile();
  if (path.exists()) {
    if ("true".equalsIgnoreCase(inline) || inline.length() == 0) {
      inlinedPaths.add(path.getPath());
    }
 else {
      String[] filters=inline.split("\\|");
      for (      String filter : filters) {
        if (filter.length() > 0) {
          inlinedPaths.add(path + "!/" + filter);
        }
      }
    }
  }
}
