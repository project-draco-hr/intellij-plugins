{
  final String descriptorPath=getRunnerParameters().getAirDescriptorPath();
  return getRunnerParameters().getAirRunMode() == AirRunnerParameters.AirRunMode.MainClass ? StringUtil.getShortName(getRunnerParameters().getMainClassName()) : getRunnerParameters().getAirRunMode() == AirRunnerParameters.AirRunMode.AppDescriptor ? descriptorPath.substring(descriptorPath.lastIndexOf('/') + 1) : "unnamed";
}
