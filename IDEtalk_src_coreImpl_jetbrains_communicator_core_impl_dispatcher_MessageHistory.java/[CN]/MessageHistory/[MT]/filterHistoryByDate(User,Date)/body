{
  List<LocalMessage> list=myHistory.readMessages(user);
  if (since != null) {
    List<LocalMessage> result=new ArrayList<LocalMessage>(list.size());
    for (    LocalMessage message : list) {
      if (message.getWhen().after(since)) {
        result.add(message);
      }
    }
    list=result;
  }
  return list;
}
