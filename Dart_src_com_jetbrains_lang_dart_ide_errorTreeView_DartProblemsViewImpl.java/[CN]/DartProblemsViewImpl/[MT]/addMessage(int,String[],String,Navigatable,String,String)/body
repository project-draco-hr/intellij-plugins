{
  myViewUpdater.execute(new Runnable(){
    @Override public void run(){
      if (navigatable != null) {
        myPanel.addMessage(type,text,groupName,navigatable,exportTextPrefix,rendererTextPrefix,null);
      }
 else {
        myPanel.addMessage(type,text,null,-1,-1,null);
      }
      updateIcon();
    }
  }
);
}
