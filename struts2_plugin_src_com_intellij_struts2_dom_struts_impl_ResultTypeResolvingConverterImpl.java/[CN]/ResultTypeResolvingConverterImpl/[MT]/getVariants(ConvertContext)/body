{
  final List<ResultType> results=new SmartList<ResultType>();
  final Processor<StrutsPackage> processor=new Processor<StrutsPackage>(){
    @Override public boolean process(    final StrutsPackage strutsPackage){
      results.addAll(strutsPackage.getResultTypes());
      return false;
    }
  }
;
  final StrutsPackageHierarchyWalker walker=new StrutsPackageHierarchyWalker(ConverterUtil.getCurrentStrutsPackage(context),processor);
  walker.walkUp();
  return results;
}
