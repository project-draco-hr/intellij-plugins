{
  StringBuilder builder=new StringBuilder();
  builder.append("[");
  builder.append("element=");
  builder.append(element + ", ");
  builder.append("offset=");
  builder.append(offset + ", ");
  builder.append("length=");
  builder.append(length + ", ");
  builder.append("children=");
  builder.append(StringUtils.join(children,", "));
  builder.append("]");
  return builder.toString();
}
