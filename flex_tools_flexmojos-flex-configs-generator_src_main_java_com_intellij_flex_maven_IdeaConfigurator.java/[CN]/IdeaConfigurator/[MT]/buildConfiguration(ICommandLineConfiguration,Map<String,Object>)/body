{
  try {
    init(parameters);
    build(configuration,ICommandLineConfiguration.class,"\n\t");
    out.append("\n\t<file-specs>\n\t\t<path-element>").append(getSourceFile().getAbsolutePath()).append("</path-element>\n\t</file-specs>");
    close();
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
