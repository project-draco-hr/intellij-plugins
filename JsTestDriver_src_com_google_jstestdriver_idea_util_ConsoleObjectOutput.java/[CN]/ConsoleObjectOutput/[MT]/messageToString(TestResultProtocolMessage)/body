{
  Object[][] args={{"phase",message.phase},{"result",message.result},{"message",message.message},{"isDryRun()",message.isDryRun()},{"stack",message.stack},{"duration",message.duration},{"log",message.log},{"testCase",message.testCase},{"testName",message.testName}};
  String s="";
  for (  Object[] a : args) {
    s+=a[0] + ": " + (a[1] == null ? a[1] : a[1].toString())+ ",\t";
  }
  return s;
}
