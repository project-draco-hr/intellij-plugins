{
  int[] positions=new int[size];
  for (int i=0; i < size; i++) {
    positions[i]=in.position();
    in.skipEntries(2);
    int flags=in.readU8();
    if ((flags & CLASS_FLAG_protected) != 0) {
      in.readU32();
    }
    long interfaceCount=in.readU32();
    in.skipEntries(interfaceCount);
    in.readU32();
    scanTraits(in);
  }
  return positions;
}
