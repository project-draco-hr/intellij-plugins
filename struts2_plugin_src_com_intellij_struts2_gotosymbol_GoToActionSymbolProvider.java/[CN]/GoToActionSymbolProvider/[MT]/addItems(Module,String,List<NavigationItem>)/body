{
  final StrutsModel strutsModel=StrutsManager.getInstance(module.getProject()).getCombinedModel(module);
  if (strutsModel == null) {
    return;
  }
  final List<Action> actions=strutsModel.findActionsByName(name,null);
  for (  final Action action : actions) {
    final NavigationItem item=createNavigationItem(action.getXmlTag(),action.getName().getStringValue() + " [" + action.getNamespace()+ "]",Struts2DomApiIcons.Action);
    result.add(item);
  }
}
