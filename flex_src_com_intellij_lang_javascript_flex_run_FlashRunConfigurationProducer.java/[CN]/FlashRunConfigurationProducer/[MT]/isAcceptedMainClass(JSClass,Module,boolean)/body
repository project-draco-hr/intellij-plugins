{
  if (jsClass == null || module == null || !BCUtils.isValidMainClass(module,null,jsClass))   return false;
  if (!allowWindowedApplicationInheritors) {
    GlobalSearchScope scope=GlobalSearchScope.moduleWithLibrariesScope(module);
    final PsiElement windowedApplicationClass1=JSResolveUtil.unwrapProxy(JSResolveUtil.findClassByQName(WINDOWED_APPLICATION_CLASS_NAME_1,scope));
    final PsiElement windowedApplicationClass2=JSResolveUtil.unwrapProxy(JSResolveUtil.findClassByQName(WINDOWED_APPLICATION_CLASS_NAME_2,scope));
    if (windowedApplicationClass1 instanceof JSClass && JSInheritanceUtil.isParentClass(jsClass,(JSClass)windowedApplicationClass1) || windowedApplicationClass2 instanceof JSClass && JSInheritanceUtil.isParentClass(jsClass,(JSClass)windowedApplicationClass2)) {
      return false;
    }
  }
  return true;
}
