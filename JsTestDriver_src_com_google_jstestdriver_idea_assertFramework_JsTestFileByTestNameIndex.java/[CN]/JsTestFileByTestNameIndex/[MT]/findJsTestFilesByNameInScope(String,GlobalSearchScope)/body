{
  final List<VirtualFile> jsTestFileList=new ArrayList<VirtualFile>(1);
  FileBasedIndex.getInstance().processValues(KEY,testCaseName,null,new FileBasedIndex.ValueProcessor<Void>(){
    @Override public boolean process(    final VirtualFile file,    final Void value){
      if (file != null) {
        jsTestFileList.add(file);
      }
      return true;
    }
  }
,scope);
  return jsTestFileList;
}
