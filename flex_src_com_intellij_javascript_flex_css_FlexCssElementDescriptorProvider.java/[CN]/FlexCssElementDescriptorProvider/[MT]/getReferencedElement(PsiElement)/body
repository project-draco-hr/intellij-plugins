{
  if (element instanceof JSClassImpl) {
    return element.getNavigationElement();
  }
 else   if (element instanceof XmlBackedJSClassImpl) {
    PsiElement parent=element.getParent();
    if (parent != null) {
      PsiFile file=parent.getContainingFile();
      if (file != null) {
        return file;
      }
    }
  }
  return element;
}
