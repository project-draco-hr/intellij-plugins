{
  final boolean asc20=bc.isPureAs() && FlexCommonUtils.containsASC20(bc.getSdk().getHomePath()) && (FlexCompilerProjectConfiguration.getInstance(module.getProject()).PREFER_ASC_20 || FlexSdkUtils.isAirSdkWithoutFlex(bc.getSdk()));
  if (asc20)   return new ASC20CompilationTask(module,bc,dependencies);
  if (builtInCompilerShell)   return new BuiltInCompilationTask(module,bc,dependencies);
  return new MxmlcCompcCompilationTask(module,bc,dependencies);
}
