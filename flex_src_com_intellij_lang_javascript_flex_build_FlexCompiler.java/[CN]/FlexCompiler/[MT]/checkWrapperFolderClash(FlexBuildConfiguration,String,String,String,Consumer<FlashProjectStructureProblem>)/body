{
  if (FileUtil.isAncestor(templateFolderPath,otherFolderPath,false)) {
    errorConsumer.consume(FlashProjectStructureProblem.createGeneralOptionProblem(Severity.ERROR,bc.getName(),FlexBundle.message("html.wrapper.folder.clash",otherFolderDescription,FileUtil.toSystemDependentName(templateFolderPath)),FlexBCConfigurable.Location.HtmlTemplatePath));
    return false;
  }
  return true;
}
