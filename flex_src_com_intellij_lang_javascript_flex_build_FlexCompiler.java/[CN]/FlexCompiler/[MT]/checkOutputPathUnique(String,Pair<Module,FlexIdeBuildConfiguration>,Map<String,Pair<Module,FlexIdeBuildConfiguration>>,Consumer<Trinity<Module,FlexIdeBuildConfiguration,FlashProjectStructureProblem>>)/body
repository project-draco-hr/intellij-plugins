{
  final String caseAwarePath=SystemInfo.isFileSystemCaseSensitive ? outputPath : outputPath.toLowerCase();
  final Pair<Module,FlexIdeBuildConfiguration> existing=outputPathToModuleAndBC.put(caseAwarePath,moduleAndBC);
  if (existing != null) {
    final String message=FlexBundle.message("same.output.files",existing.second.getName(),existing.first.getName(),FileUtil.toSystemDependentName(outputPath));
    errorConsumer.consume(Trinity.create(moduleAndBC.first,moduleAndBC.second,FlashProjectStructureProblem.createGeneralOptionProblem(moduleAndBC.second.getName(),message,FlexIdeBCConfigurable.Location.OutputFileName)));
  }
}
