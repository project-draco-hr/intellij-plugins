{
  if (ModuleType.get(module) instanceof FlexModuleType) {
    for (    final FlexIdeBuildConfiguration config : FlexBuildConfigurationManager.getInstance(module).getBuildConfigurations()) {
      compilationTasks.add(builtInCompiler ? new BuiltInCompilationTask(module,config) : new MxmlcCompcCompilationTask(module,config));
    }
  }
}
