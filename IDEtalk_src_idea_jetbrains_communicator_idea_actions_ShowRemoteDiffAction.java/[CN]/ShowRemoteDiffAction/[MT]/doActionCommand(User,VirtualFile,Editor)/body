{
  VFile vFile=VFSUtil.createFileFrom(file,editor.getProject());
  if (vFile == null) {
    LOG.info("Unable to get file " + file);
    return;
  }
  ShowDiffCommand command=((ShowDiffCommand)Pico.getCommandManager().getCommand(ShowDiffCommand.class,BaseAction.getContainer(editor.getProject())));
  command.setUser(user);
  command.setVFile(vFile);
  command.execute();
}
