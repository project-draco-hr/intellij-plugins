{
  if (message == null)   message="<no error message>";
  if (stackTrace == null)   stackTrace="<no stack trace>";
  LOG.warn("Dart analysis server, SDK version " + mySdkVersion + ", server version "+ myServerVersion+ ", "+ (isFatal ? "FATAL " : "")+ "error: "+ message+ "\n"+ stackTrace);
  if (isFatal) {
    onServerStopped();
  }
}
