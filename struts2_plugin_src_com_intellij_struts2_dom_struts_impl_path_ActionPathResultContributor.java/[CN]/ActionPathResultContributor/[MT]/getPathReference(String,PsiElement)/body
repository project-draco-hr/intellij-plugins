{
  final StrutsModel model=StrutsManager.getInstance(element.getProject()).getModelByFile((XmlFile)element.getContainingFile());
  if (model == null) {
    return null;
  }
  final String currentPackage=getNamespace(element);
  if (currentPackage == null) {
    return null;
  }
  final List<String> actionExtensions=StrutsConstantHelper.getActionExtensions(element);
  if (actionExtensions.isEmpty()) {
    return null;
  }
  final PsiElement actionTag=resolveActionPath(element,currentPackage,model,actionExtensions);
  if (actionTag == null) {
    return null;
  }
  return new PathReference(path,new ConstantFunction<PathReference,Icon>(StrutsIcons.ACTION)){
    @Override public PsiElement resolve(){
      return actionTag;
    }
  }
;
}
