{
  try {
    char[] chars=FileUtil.loadFileText(ignoreList);
    String[] lines=new String(chars).split("[\r\n]");
    myIgnored.clear();
    for (    String line : lines) {
      if (StringUtil.isNotEmpty(line)) {
        myIgnored.add(line.toLowerCase());
      }
    }
  }
 catch (  IOException e) {
    LOG.error(e,e);
  }
}
