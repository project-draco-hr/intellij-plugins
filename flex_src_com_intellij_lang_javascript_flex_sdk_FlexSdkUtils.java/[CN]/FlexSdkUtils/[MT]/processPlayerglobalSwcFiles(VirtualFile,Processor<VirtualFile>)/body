{
  VirtualFile playerglobalSwcFile;
  for (  final VirtualFile subDir : playerDir.getChildren()) {
    if (subDir.isDirectory() && (playerglobalSwcFile=subDir.findChild("playerglobal.swc")) != null && PLAYER_FOLDER_PATTERN.matcher(subDir.getName()).matches()) {
      if (!processor.process(playerglobalSwcFile)) {
        break;
      }
    }
  }
}
