{
  int i=offset;
  char endChar;
  if (c == '{') {
    endChar='}';
  }
 else {
    endChar=c;
  }
  while (i < chars.length() && (chars.charAt(i) != '{' && chars.charAt(i) != endChar && chars.charAt(i) != '\n')) {
    i++;
  }
  if (i + 1 < chars.length() && chars.charAt(i) == endChar && chars.charAt(i + 1) == '}') {
    return true;
  }
  return false;
}
