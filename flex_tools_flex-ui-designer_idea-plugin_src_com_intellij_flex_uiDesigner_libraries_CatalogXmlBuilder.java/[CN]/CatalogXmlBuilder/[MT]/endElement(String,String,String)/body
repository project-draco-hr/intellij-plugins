{
  if (processDependencies && name.equals("script")) {
    if (dependencies.isEmpty()) {
      definition.resolved=ResolvedState.YES;
    }
 else {
      definition.dependencies=dependencies.toArray(new String[dependencies.size()]);
      dependencies.clear();
    }
    definition=null;
    processDependencies=false;
    defProcessing=false;
  }
}
