{
  TextFieldWithHistoryWithBrowseButton field=SwingHelper.createTextFieldWithHistoryWithBrowseButton(project,PhoneGapBundle.message("phonegap.conf.executable.name"),FileChooserDescriptorFactory.createSingleFileNoJarsDescriptor(),new NotNullProducer<List<String>>(){
    @NotNull @Override public List<String> produce(){
      return getDefaultExecutablePaths();
    }
  }
);
  String executablePath=PhoneGapSettings.getInstance().getExecutablePath();
  setDefaultValue(field,executablePath);
  return field;
}
