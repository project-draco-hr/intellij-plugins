{
  BundleManifest bundleManifest=getManifest(Manifest1);
  assertThat(bundleManifest.getExportedPackage("foo.bar.baz"),is("foo.bar.baz"));
  assertThat(bundleManifest.getExportedPackage("foo.bar.baz"),is("foo.bar.baz"));
  assertThat(bundleManifest.getExportedPackage("foo.bar.oops"),is((String)null));
}
