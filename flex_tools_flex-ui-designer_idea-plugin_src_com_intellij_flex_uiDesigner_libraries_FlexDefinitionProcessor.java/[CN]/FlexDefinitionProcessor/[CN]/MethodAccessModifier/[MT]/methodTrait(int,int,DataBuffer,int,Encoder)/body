{
  if (skipMethod != null && isOverridenMethod(traitKind)) {
    if (encoder.skipMethod(skipMethod,name,in)) {
      skipMethod=null;
      return true;
    }
  }
 else   if (skipSetter != null && isSetter(traitKind) && encoder.clearMethodBody(skipSetter,name,in,methodInfo)) {
    skipSetter=null;
    return false;
  }
  return false;
}
