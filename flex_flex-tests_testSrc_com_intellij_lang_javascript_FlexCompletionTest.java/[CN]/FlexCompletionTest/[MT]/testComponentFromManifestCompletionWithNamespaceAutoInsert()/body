{
  final String name=getTestName(false);
  JSTestUtils.modifyBuildConfiguration(myModule,new Consumer<ModifiableFlexBuildConfiguration>(){
    public void consume(    final ModifiableFlexBuildConfiguration bc){
      final String manifest=getTestDataPath() + "/" + getBasePath()+ "/"+ name+ "_manifest.xml";
      bc.getCompilerOptions().setAllOptions(Collections.singletonMap("compiler.namespaces.namespace","schema://www.MyNamespace.com/2010\t" + manifest));
    }
  }
);
  final String testName=getTestName(false);
  VirtualFile[] files=new VirtualFile[]{getVirtualFile(BASE_PATH + testName + ".mxml"),getVirtualFile(BASE_PATH + testName + "_other.as")};
  doTestForFiles(files,"",MXML_EXTENSION);
}
