{
  StringBuilder builder=new StringBuilder();
  for (int i=removePrefix(s), n=s.length(); i < n; i++) {
    char c=s.charAt(i);
    if (Character.isUpperCase(c)) {
      builder.append('-').append(Character.toLowerCase(c));
    }
 else {
      builder.append(c);
    }
  }
  return builder.toString();
}
