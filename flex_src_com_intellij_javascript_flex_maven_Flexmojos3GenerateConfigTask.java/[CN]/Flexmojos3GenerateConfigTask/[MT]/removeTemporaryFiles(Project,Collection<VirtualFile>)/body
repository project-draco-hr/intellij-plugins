{
  MavenUtil.invokeAndWaitWriteAction(project,new Runnable(){
    public void run(){
      for (      VirtualFile file : files) {
        try {
          if (file.isValid() && file.getLength() == TEMPORARY_FILE_CONTENT.length() && new String(file.contentsToByteArray()).equals(TEMPORARY_FILE_CONTENT)) {
            file.delete(Flexmojos3GenerateConfigTask.class);
          }
        }
 catch (        IOException e) {
        }
      }
    }
  }
);
}
