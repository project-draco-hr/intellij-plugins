{
  int count=out.size();
  final byte[] bytes=out.getBuffer(8);
  bytes[count++]=(byte)(v >>> 56);
  bytes[count++]=(byte)(v >>> 48);
  bytes[count++]=(byte)(v >>> 40);
  bytes[count++]=(byte)(v >>> 32);
  bytes[count++]=(byte)(v >>> 24);
  bytes[count++]=(byte)(v >>> 16);
  bytes[count++]=(byte)(v >>> 8);
  bytes[count]=(byte)(v);
}
