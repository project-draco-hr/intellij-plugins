{
  return new Analyzer(){
    @Override public String getProperty(    String key){
      return props.get(key);
    }
    @Override public String getProperty(    String key,    String fallback){
      return props.containsKey(key) ? key : fallback;
    }
    @Override public void setProperty(    String key,    String value){
      props.put(key,value);
    }
  }
;
}
