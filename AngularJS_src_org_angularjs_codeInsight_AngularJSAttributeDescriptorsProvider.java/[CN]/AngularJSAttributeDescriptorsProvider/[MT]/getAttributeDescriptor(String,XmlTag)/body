{
  final String attributeName=normalizeAttributeName(attrName);
  if (xmlTag != null) {
    final Project project=xmlTag.getProject();
    return AngularIndexUtil.getAllKeys(AngularDirectivesIndex.INDEX_ID,project).contains(attributeName) || AngularIndexUtil.getAllKeys(AngularDirectivesDocIndex.INDEX_ID,project).contains(attributeName) ? createDescriptor(project,attributeName) : null;
  }
  return null;
}
