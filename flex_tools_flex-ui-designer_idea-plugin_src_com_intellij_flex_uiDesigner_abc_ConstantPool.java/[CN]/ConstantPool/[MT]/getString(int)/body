{
  if (index == 0) {
    return null;
  }
  int pos=positions[IndexHistory.STRING][index];
  int originalPos=in.position();
  in.seek(pos);
  String value=in.readString(in.readU32());
  in.seek(originalPos);
  if (value != null) {
    return value;
  }
 else {
    throw new DecoderException("abc Decoder Erro: problem reading UTF-8 encoded strings.");
  }
}
