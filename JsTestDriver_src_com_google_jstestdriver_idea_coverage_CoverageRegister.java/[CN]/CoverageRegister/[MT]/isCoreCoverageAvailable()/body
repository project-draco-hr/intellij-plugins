{
  for (  String aClass : REQUIRED_CLASSES) {
    try {
      Class.forName(aClass);
    }
 catch (    ClassNotFoundException e) {
      return false;
    }
  }
  try {
    for (    String extensionPointName : EXTENSION_POINT_NAMES) {
      Extensions.getRootArea().getExtensionPoint(extensionPointName);
    }
  }
 catch (  Exception e) {
    return false;
  }
  return true;
}
