{
  return new TreeBasedStructureViewBuilder(){
    @NotNull @Override public StructureViewModel createStructureViewModel(){
      PsiElement root=PsiTreeUtil.getChildOfType(psiFile,GherkinFeature.class);
      if (root == null) {
        root=psiFile;
      }
      return new StructureViewModelBase(psiFile,new GherkinStructureViewElement(root)).withSuitableClasses(GherkinFile.class,GherkinFeature.class,GherkinStepsHolder.class,GherkinStep.class);
    }
  }
;
}
