{
  final Predicate<DartCallExpression> callExpressionPredicate=new Predicate<DartCallExpression>(){
    @Override public boolean apply(    final DartCallExpression expression){
      return DartTestLocationProvider.testLabelMatches(expression,testLabel);
    }
  }
;
  return scope == Scope.METHOD ? !Collections2.filter(myTestCalls,callExpressionPredicate).isEmpty() : !Collections2.filter(myGroupCalls,callExpressionPredicate).isEmpty();
}
