{
  final String type;
switch (descriptor) {
case 'Z':
    type="bool";
  break;
case 'B':
type="Byte";
break;
case 'C':
type="char";
break;
case 'D':
type="double";
break;
case 'F':
type="float";
break;
case 'I':
type="int";
break;
case 'J':
type="long";
break;
default :
type="void";
}
typeBuilder.append(type);
}
