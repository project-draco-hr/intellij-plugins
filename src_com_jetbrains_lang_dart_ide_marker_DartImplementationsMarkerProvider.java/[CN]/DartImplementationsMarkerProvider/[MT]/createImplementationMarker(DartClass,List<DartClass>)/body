{
  final DartComponentName componentName=dartClass.getComponentName();
  return new LineMarkerInfo<PsiElement>(componentName,componentName.getTextRange(),dartClass instanceof DartInterfaceDefinition ? AllIcons.Gutter.ImplementedMethod : AllIcons.Gutter.OverridenMethod,Pass.UPDATE_ALL,new Function<PsiElement,String>(){
    @Override public String fun(    PsiElement element){
      return DaemonBundle.message("method.is.implemented.too.many");
    }
  }
,new GutterIconNavigationHandler<PsiElement>(){
    @Override public void navigate(    MouseEvent e,    PsiElement elt){
      PsiElementListNavigator.openTargets(e,DartResolveUtil.getComponentNames(items).toArray(new NavigatablePsiElement[items.size()]),DaemonBundle.message("navigation.title.subclass",dartClass.getName(),items.size()),"Superclasses of " + dartClass.getName(),new DefaultPsiElementCellRenderer());
    }
  }
,GutterIconRenderer.Alignment.RIGHT);
}
