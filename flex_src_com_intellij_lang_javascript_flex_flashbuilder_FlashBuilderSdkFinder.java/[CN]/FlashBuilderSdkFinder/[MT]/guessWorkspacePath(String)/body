{
  VirtualFile dir=LocalFileSystem.getInstance().findFileByPath(selectedPath);
  if (dir != null && !dir.isDirectory()) {
    dir=dir.getParent();
  }
  while (dir != null) {
    if (FlashBuilderProjectFinder.isFlashBuilderWorkspace(dir)) {
      return dir.getPath();
    }
    dir=dir.getParent();
  }
  return null;
}
