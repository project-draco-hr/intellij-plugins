{
  final SdkType sdkType=projectType == FlashBuilderProject.ProjectType.AIR ? (SdkType)AirSdkType.getInstance() : projectType == FlashBuilderProject.ProjectType.MobileAIR ? (SdkType)AirMobileSdkType.getInstance() : (SdkType)FlexSdkType.getInstance();
  return FlexSdkUtils.createOrGetSdk(sdkType,sdkHomePath);
}
