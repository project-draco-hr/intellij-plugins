  result_=result_ && userDefinableOperator(builder_,level_ + 1);
  pinned_=result_;
  result_=result_ && report_error_(builder_,formalParameterList(builder_,level_ + 1));
  result_=pinned_ && operatorDeclaration_5(builder_,level_ + 1) && result_;
