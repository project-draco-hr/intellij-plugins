{
  if (!recursion_guard_(builder_,level_,"ifStatement_5_0"))   return false;
  boolean result_=false;
  Marker marker_=builder_.mark();
  result_=consumeToken(builder_,ELSE);
  result_=result_ && statement(builder_,level_ + 1);
  if (!result_) {
    marker_.rollbackTo();
  }
 else {
    marker_.drop();
  }
  return result_;
}
