{
  if (!recursion_guard_(b,l,"forLoopParts_2"))   return false;
  boolean r;
  Marker m=enter_section_(b);
  r=forLoopParts_2_0(b,l + 1);
  r=r && consumeToken(b,SEMICOLON);
  r=r && forLoopParts_2_2(b,l + 1);
  r=r && consumeToken(b,SEMICOLON);
  r=r && forLoopParts_2_4(b,l + 1);
