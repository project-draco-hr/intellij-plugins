{
  if (!recursion_guard_(builder_,level_,"id"))   return false;
  if (!nextTokenIs(builder_,IDENTIFIER))   return false;
  boolean result_=false;
  Marker marker_=builder_.mark();
  result_=consumeToken(builder_,IDENTIFIER);
  if (result_) {
    marker_.done(ID);
  }
 else {
    marker_.rollbackTo();
  }
  return result_;
}
