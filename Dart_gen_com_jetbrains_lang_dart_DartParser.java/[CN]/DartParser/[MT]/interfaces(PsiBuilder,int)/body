{
  if (!recursion_guard_(b,l,"interfaces"))   return false;
  if (!nextTokenIs(b,IMPLEMENTS))   return false;
  boolean r, p;
  Marker m=enter_section_(b,l,_NONE_,null);
  r=consumeToken(b,IMPLEMENTS);
  p=r;
  r=r && typeList(b,l + 1);
  exit_section_(b,l,m,INTERFACES,r,p,null);
  return r || p;
}
