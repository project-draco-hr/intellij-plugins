{
  if (!recursion_guard_(b,l,"isExpression"))   return false;
  if (!nextTokenIs(b,IS))   return false;
  boolean r;
  Marker m=enter_section_(b,l,_LEFT_,null);
  r=consumeToken(b,IS);
  r=r && isExpression_1(b,l + 1);
  r=r && type(b,l + 1);
  exit_section_(b,l,m,IS_EXPRESSION,r,false,null);
  return r;
}
