{
  myFiles.clear();
  myJars.clear();
  final List<DomFileElement<StrutsRoot>> elements=DomService.getInstance().getFileElements(StrutsRoot.class,module.getProject(),GlobalSearchScope.moduleWithDependenciesAndLibrariesScope(module));
  for (  final DomFileElement<StrutsRoot> element : elements) {
    final XmlFile file=element.getFile();
    final VirtualFile jar=JarFileSystem.getInstance().getVirtualFileForJar(file.getVirtualFile());
    if (jar != null) {
      myJars.putValue(jar,file);
    }
 else {
      final Module module=ModuleUtil.findModuleForPsiElement(file);
      if (module != null) {
        myFiles.putValue(module,file);
      }
    }
  }
}
