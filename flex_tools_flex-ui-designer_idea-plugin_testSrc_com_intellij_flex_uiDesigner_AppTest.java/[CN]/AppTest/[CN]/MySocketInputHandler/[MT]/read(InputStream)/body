{
  createReader(inputStream);
  while (info.count != 0) {
    try {
      info.result.set(reader.readBoolean() ? null : reader.readUTF());
    }
 catch (    IOException e) {
      if (!(e instanceof SocketException)) {
        LOG.error(e);
      }
      break;
    }
 finally {
      info.lock.countDown();
      info.count--;
    }
  }
}
