{
  boolean result=true;
  try {
    result=super.processCommand(command);
  }
 catch (  Throwable e) {
    if (!(e instanceof SocketException)) {
      LOG.error(e);
      result=false;
      info.fail.set(true);
    }
  }
 finally {
    if (!result) {
      info.lock.countDown();
    }
  }
  return true;
}
