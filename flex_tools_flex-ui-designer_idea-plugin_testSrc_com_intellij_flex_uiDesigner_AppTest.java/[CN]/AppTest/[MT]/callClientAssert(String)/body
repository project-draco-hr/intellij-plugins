{
  info.lock=new CountDownLatch(1);
  Client client=Client.getInstance();
  AmfOutputStream out=client.getOut();
  out.write(1);
  out.writeAmfUtf(methodName,false);
  out.write(3);
  client.flush();
  await();
  if (!info.result.get().equals(PASSED)) {
    fail(info.result.get());
  }
  info.result.set("");
}
