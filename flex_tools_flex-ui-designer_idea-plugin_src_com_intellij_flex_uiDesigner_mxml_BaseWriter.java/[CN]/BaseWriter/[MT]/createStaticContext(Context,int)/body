{
  if (parentContext == null || parentContext.getBackSibling() == null) {
    return new StaticObjectContext(referencePosition,out,nullContext.getId(),parentContext instanceof InnerComponentContext ? parentContext.getScope() : rootScope);
  }
 else {
    return parentContext.getBackSibling().reinitialize(referencePosition,nullContext.getId());
  }
}
