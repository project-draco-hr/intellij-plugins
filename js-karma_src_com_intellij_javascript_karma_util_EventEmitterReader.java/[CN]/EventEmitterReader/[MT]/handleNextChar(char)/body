{
  if (mySkipLF && ch != NL) {
    myCurrentLine.append('\r');
  }
  if (ch == '\r') {
    mySkipLF=true;
  }
 else {
    mySkipLF=false;
    myCurrentLine.append(ch);
  }
  if (ch == NL) {
    String line=myCurrentLine.toString();
    if (!handleLineAsEvent(line)) {
      myCharQueue.add(line);
    }
    myCurrentLine.setLength(0);
  }
}
