{
  final JpsModule module=((JpsFlexBuildConfiguration)myParent.getParent().getParent()).getModule();
  for (  JpsLibrary library : module.getLibraryCollection().getLibraries()) {
    if (library.getType() == JpsFlexLibraryType.INSTANCE && myLibraryId.equals(((JpsTypedLibrary<JpsSimpleElement<JpsFlexLibraryProperties>>)library).getProperties().getData().getLibraryId())) {
      return library;
    }
  }
  return null;
}
