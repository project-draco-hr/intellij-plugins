{
  myLexer.start(text);
  StringBuilder actual=new StringBuilder();
  IElementType token;
  while ((token=myLexer.getTokenType()) != null) {
    actual.append(token).append(" ('").append(myLexer.getTokenText()).append("')\n");
    myLexer.advance();
  }
  assertEquals(expected.trim(),actual.toString().trim());
}
