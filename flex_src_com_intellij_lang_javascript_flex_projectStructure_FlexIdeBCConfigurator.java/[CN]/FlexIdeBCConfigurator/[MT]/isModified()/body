{
  if (myModified)   return true;
  for (  final List<FlexIdeBCConfigurable> configurables : myModuleToConfigurablesMap.values()) {
    for (    final FlexIdeBCConfigurable configurable : configurables) {
      if (configurable.isModified()) {
        return true;
      }
    }
  }
  return false;
}
