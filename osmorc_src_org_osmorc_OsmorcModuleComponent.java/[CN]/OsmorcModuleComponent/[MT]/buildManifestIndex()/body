{
  OsmorcFacet facet=OsmorcFacet.getInstance(myModule);
  if (facet != null) {
    myApplication.invokeLater(new Runnable(){
      @Override public void run(){
        if (myModule.isDisposed())         return;
        myBundleManager.reindex(myModule);
      }
    }
,myModule.getDisposed());
  }
}
