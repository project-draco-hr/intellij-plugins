{
  if (!mv.equals("2")) {
    clauses.clear();
  }
 else {
    for (    ParsedHeaderClause clause : clauses) {
      Object value=clause.m_attrs.get(Constants.BUNDLE_VERSION_ATTRIBUTE);
      if (value != null) {
        clause.m_attrs.put(Constants.BUNDLE_VERSION_ATTRIBUTE,VersionRange.parse(value.toString()));
      }
    }
  }
  return clauses;
}
