{
  List<ParsedHeaderClause> clauses=parseStandardHeader(header);
  String result=null;
  if (clauses.size() == 1) {
    for (    Entry<String,String> entry : clauses.get(0).m_dirs.entrySet()) {
      if (Constants.EXTENSION_DIRECTIVE.equals(entry.getKey())) {
        if (FelixConstants.SYSTEM_BUNDLE_SYMBOLICNAME.equals(clauses.get(0).m_paths.get(0)) || Constants.SYSTEM_BUNDLE_SYMBOLICNAME.equals(clauses.get(0).m_paths.get(0))) {
          return entry.getValue();
        }
 else {
          throw new BundleException("Only the system bundle can have extension bundles.");
        }
      }
    }
  }
  return result;
}
