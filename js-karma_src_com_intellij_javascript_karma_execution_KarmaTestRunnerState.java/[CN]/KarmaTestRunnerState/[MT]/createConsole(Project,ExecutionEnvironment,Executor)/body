{
  KarmaRunConfiguration runConfiguration=(KarmaRunConfiguration)env.getRunProfile();
  TestConsoleProperties testConsoleProperties=new SMTRunnerConsoleProperties(new RuntimeConfigurationProducer.DelegatingRuntimeConfiguration<KarmaRunConfiguration>(runConfiguration),FRAMEWORK_NAME,executor);
  SMTRunnerConsoleView smtConsoleView=SMTestRunnerConnectionUtil.createConsoleWithCustomLocator(FRAMEWORK_NAME,testConsoleProperties,env.getRunnerSettings(),env.getConfigurationSettings(),new KarmaTestLocationProvider(),true,null);
  Disposer.register(project,smtConsoleView);
  return smtConsoleView;
}
