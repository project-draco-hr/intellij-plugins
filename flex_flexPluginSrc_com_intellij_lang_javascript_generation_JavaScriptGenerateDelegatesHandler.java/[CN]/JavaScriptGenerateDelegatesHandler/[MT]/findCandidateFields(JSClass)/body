{
  Collection<JSVariable> result=new ArrayList<JSVariable>();
  for (  JSVariable field : clazz.getFields()) {
    JSClass fieldType=field.getType().resolveClass();
    if (fieldType != null && !ArrayUtil.contains(fieldType.getQualifiedName(),PRIMITIVE_TYPES) && !JSInheritanceUtil.isParentClass(clazz,fieldType,false)) {
      result.add(field);
    }
  }
  return result;
}
