{
  if (outputType == FlexIdeBuildConfiguration.OutputType.Library) {
    return LinkageType.External;
  }
 else   if (pureAS) {
    return LinkageType.Merged;
  }
 else   if (targetPlatform == FlexIdeBuildConfiguration.TargetPlatform.Web) {
    return LinkageType.RSL;
  }
 else {
    return LinkageType.Merged;
  }
}
