{
  String message=sdk == null ? FlashUIDesignerBundle.message("module.sdk.is.not.specified",module.getName()) : FlashUIDesignerBundle.message("module.sdk.is.not.compatible",sdk.getVersionString(),module.getName());
  notifyUser(debug,message,module.getProject(),new Consumer<String>(){
    @Override public void consume(    String id){
      if ("edit".equals(id)) {
        FlexSdkUtils.openModuleConfigurable(module);
      }
 else {
        LOG.error("unexpected id: " + id);
      }
    }
  }
);
}
