{
  return new DataExternalizer<List<String>>(){
    @Override public void save(    DataOutput out,    List<String> value) throws IOException {
      out.writeInt(value.size());
      for (      String path : value) {
        out.writeUTF(path);
      }
    }
    @Override public List<String> read(    DataInput in) throws IOException {
      final int size=in.readInt();
      final List<String> result=new ArrayList<String>(size);
      for (int i=0; i < size; ++i) {
        result.add(in.readUTF());
      }
      return result;
    }
  }
;
}
