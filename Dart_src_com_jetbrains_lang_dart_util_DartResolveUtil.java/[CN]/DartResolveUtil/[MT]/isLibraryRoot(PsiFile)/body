{
  for (  PsiElement root : findDartRoots(psiFile)) {
    DartPsiCompositeElement oneOfLibraryStatement=PsiTreeUtil.getChildOfAnyType(root,DartLibraryStatement.class,DartSourceStatement.class,DartImportStatement.class,DartNativeStatement.class);
    if (oneOfLibraryStatement != null || hasMainFunction(psiFile)) {
      return true;
    }
  }
  return false;
}
