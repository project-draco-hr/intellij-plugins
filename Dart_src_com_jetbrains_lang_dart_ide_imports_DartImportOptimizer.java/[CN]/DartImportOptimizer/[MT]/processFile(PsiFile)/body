{
  return new Runnable(){
    @Override public void run(){
      final String filePath=file.getVirtualFile().getPath();
      final SourceFileEdit fileEdit=DartAnalysisServerService.getInstance().edit_organizeDirectives(filePath);
      if (fileEdit != null) {
        AssistUtils.applyFileEdit(file.getProject(),fileEdit);
      }
    }
  }
;
}
