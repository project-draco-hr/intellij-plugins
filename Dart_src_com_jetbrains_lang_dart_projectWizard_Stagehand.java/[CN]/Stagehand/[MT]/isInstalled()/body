{
  try {
    final Output output=new PubRunner().runSync("global","list");
    if (output.getExitCode() != 0) {
      return false;
    }
    String[] lines=output.getStdout().split("\n");
    for (    String line : lines) {
      if (line.startsWith("stagehand ")) {
        return true;
      }
    }
  }
 catch (  StagehandException e) {
    LOG.error(e);
  }
  return false;
}
