{
  doBackspaceTest("var foo = \"<caret> \"","var foo = <caret> \"");
  doBackspaceTest("var foo = \"<caret>\"","var foo = <caret>");
  doBackspaceTest("var foo = '<caret>a'","var foo = <caret>a'");
  doBackspaceTest("import '<caret>'","import <caret>");
  doBackspaceTest("var foo = \"\"<caret>","var foo = \"<caret>");
  doBackspaceTest("var foo = \" '<caret>' \"","var foo = \" <caret>' \"");
  doBackspaceTest("var foo = '\"<caret>\"'","var foo = '<caret>\"'");
}
