{
  if (index == 0) {
    return 0;
  }
 else {
    int insertionIndex=getInsertionIndex(poolIndex,kind,index);
    int newIndex=getNewIndex(insertionIndex);
    if (newIndex == 0) {
      return map[insertionIndex]=decodeOnDemand(poolIndex,kind,index,-1);
    }
 else {
      return newIndex;
    }
  }
}
