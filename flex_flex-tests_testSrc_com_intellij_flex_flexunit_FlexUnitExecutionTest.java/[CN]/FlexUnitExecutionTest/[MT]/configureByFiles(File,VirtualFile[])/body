{
  final Ref<VirtualFile> result=new Ref<VirtualFile>();
  UIUtil.invokeAndWaitIfNeeded(new Runnable(){
    @Override public void run(){
      try {
        result.set(FlexUnitExecutionTest.super.configureByFiles(projectRoot,vFiles));
      }
