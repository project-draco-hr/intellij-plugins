{
  final PsiReferenceProvider propertiesProvider=CommonReferenceProviderTypes.PROPERTIES_FILE_KEY_PROVIDER.getProvider();
  wrappedPropertiesProvider=new PsiReferenceProvider(){
    @NotNull @Override public PsiReference[] getReferencesByElement(    final @NotNull PsiElement psiElement,    final @NotNull ProcessingContext processingContext){
      final StrutsFacet facet=StrutsFacet.getInstance(psiElement);
      return facet != null && !facet.getConfiguration().isPropertiesKeysDisabled() ? propertiesProvider.getReferencesByElement(psiElement,processingContext) : PsiReference.EMPTY_ARRAY;
    }
  }
;
}
