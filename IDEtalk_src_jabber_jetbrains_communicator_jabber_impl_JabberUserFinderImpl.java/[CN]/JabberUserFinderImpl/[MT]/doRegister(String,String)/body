{
  myIdeFacade.runOnPooledThread(new Runnable(){
    public void run(){
      try {
        URL url=new URL(myRegistryUrl + "?user=" + jabberUserId+ "&id="+ currentProjectId);
        url.getContent();
      }
 catch (      Exception e) {
        LOG.debug(e.getMessage(),e);
      }
    }
  }
);
}
