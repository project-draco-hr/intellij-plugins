{
  new Thread("Register User/Project in IDEtalk registry"){
    public void run(){
      try {
        URL url=new URL(myRegistryUrl + "?user=" + jabberUserId+ "&id="+ currentProjectId);
        url.getContent();
      }
 catch (      Exception e) {
        LOG.debug(e.getMessage(),e);
      }
    }
  }
.start();
}
