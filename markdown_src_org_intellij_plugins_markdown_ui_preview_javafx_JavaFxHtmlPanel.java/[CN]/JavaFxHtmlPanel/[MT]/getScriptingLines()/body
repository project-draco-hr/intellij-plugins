{
  try {
    final String libPath=MarkdownPluginUtil.getMarkdownPluginPath() + "/lib";
    return new StringBuilder().append("<script src=\"").append(new File(libPath + "/scrollToElement.js").toURI()).append("\"></script>\n").append("<script src=\"").append(new File(libPath + "/processLinks.js").toURI()).append("\"></script>\n").toString();
  }
 catch (  FileNotFoundException e) {
    return "";
  }
}
