{
  final String s=StringUtil.join(myNamespaceAndManifestFileInfoList,new Function<FlexBuildConfiguration.NamespaceAndManifestFileInfo,String>(){
    public String fun(    final FlexBuildConfiguration.NamespaceAndManifestFileInfo info){
      return info.NAMESPACE;
    }
  }
,", ");
  myCustomNamespacesTextWithBrowse.setText(s);
}
