{
  if (StringUtil.isNotEmpty(repositoryRootPath) && StringUtil.isNotEmpty(groupId) && StringUtil.isNotEmpty(artifactId)&& StringUtil.isNotEmpty(version)) {
    final StringBuilder classpathEntry=new StringBuilder();
    classpathEntry.append(repositoryRootPath).append('/').append(groupId.replace('.','/')).append('/').append(artifactId).append('/').append(version).append('/').append(artifactId).append('-').append(version).append(".jar");
    myFlexCompilerClasspath.add(classpathEntry.toString());
  }
}
