{
  JPanel panel=new JPanel(new GridBagLayout());
  myLabel.setDisplayedMnemonic('e');
{
    GridBagConstraints c=new GridBagConstraints(0,0,1,1,0.0,0.0,GridBagConstraints.WEST,GridBagConstraints.NONE,new Insets(UIUtil.DEFAULT_VGAP,0,0,UIUtil.DEFAULT_HGAP),0,0);
    panel.add(myLabel,c);
  }
{
    GridBagConstraints c=new GridBagConstraints(1,0,1,1,1.0,1.0,GridBagConstraints.WEST,GridBagConstraints.HORIZONTAL,new Insets(UIUtil.DEFAULT_VGAP,0,0,0),0,0);
    myTestCaseNameTextField=new JTextField();
    panel.add(myTestCaseNameTextField,c);
    myLabel.setLabelFor(myTestCaseNameTextField);
  }
  return panel;
}
