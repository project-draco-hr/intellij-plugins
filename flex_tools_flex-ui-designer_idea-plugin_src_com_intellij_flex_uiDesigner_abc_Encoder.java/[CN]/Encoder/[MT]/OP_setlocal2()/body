{
  if (opcodePass == 1) {
    if (opat(2) == OP_getlocal2 && opat(1) == OP_increment_i) {
      rewind(2);
      OP_inclocal_i(2);
      return;
    }
    if (opat(2) == OP_getlocal2 && opat(1) == OP_increment) {
      rewind(2);
      OP_inclocal(2);
      return;
    }
    beginop(OP_setlocal2);
  }
}
