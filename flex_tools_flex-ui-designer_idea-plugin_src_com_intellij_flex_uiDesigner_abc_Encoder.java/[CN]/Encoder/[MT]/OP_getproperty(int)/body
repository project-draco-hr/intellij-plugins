{
  if (opcodePass == 1) {
    if (opat(1) == OP_findpropstrict && readIntAt(1) == history.getIndex(poolIndex,IndexHistory.cp_mn,index)) {
      rewind(1);
      OP_getlex(index);
      return;
    }
    beginop(OP_getproperty);
    opcodes.writeU32(history.getIndex(poolIndex,IndexHistory.cp_mn,index));
  }
}
