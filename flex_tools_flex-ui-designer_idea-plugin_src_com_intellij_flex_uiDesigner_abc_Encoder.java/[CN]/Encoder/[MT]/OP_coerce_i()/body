{
  if (opcodePass == 1) {
switch (opat(1)) {
case OP_coerce_i:
case OP_convert_i:
case OP_increment_i:
case OP_decrement_i:
case OP_pushbyte:
case OP_pushshort:
case OP_pushint:
case OP_bitand:
case OP_bitor:
case OP_bitxor:
case OP_lshift:
case OP_rshift:
case OP_add_i:
case OP_subtract_i:
case OP_multiply_i:
case OP_bitnot:
      return;
  }
  beginop(OP_coerce_i);
}
}
