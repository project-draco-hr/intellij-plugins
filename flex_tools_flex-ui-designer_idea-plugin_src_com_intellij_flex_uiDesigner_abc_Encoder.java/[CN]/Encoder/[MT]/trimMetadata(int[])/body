{
  if (metadata == null) {
    return null;
  }
  tempMetadataList.resetQuick();
  for (  int aMetadata : metadata) {
    int new_index=metadataInfo.getIndex(poolIndex,aMetadata);
    if (new_index != -1) {
      tempMetadataList.add(new_index);
    }
  }
  return tempMetadataList.isEmpty() ? null : tempMetadataList;
}
