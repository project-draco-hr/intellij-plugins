{
  final Sdk sdk45=JSTestUtils.createSdk(JSTestUtils.getPathToCompleteFlexSdk("4.5"),null,false);
  JSTestUtils.addFlexLibrary(false,myModule,"foobar",true,getTestDataPath(),BASE_PATH + getTestName(false) + ".swc",null,null,LinkageType.Merged);
  JSTestUtils.modifyConfigs(myProject,new Consumer<FlexProjectConfigurationEditor>(){
    @Override public void consume(    final FlexProjectConfigurationEditor e){
      ModifiableFlexBuildConfiguration bc1=e.getConfigurations(myModule)[0];
      JSTestUtils.setSdk(bc1,sdk45);
    }
  }
);
  doTestFor(true,getTestName(false) + ".js2");
}
