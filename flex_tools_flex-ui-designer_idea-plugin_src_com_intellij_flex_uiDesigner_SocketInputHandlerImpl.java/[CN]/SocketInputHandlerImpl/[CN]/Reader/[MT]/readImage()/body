{
  final int w=readUnsignedShort();
  if (w == 0) {
    return null;
  }
  final int h=readUnsignedShort();
  int l=w * h * 4;
  final byte[] argb=FileUtil.loadBytes(this,l);
  final byte[] bgr=new byte[w * h * 3];
  for (int i=0, j=0; i < l; i+=4) {
    bgr[j++]=argb[i + 3];
    bgr[j++]=argb[i + 2];
    bgr[j++]=argb[i + 1];
  }
  return new BufferedImage(COLOR_MODER,Raster.createInterleavedRaster(new DataBufferByte(bgr,bgr.length),w,h,w * 3,3,new int[]{2,1,0},null),false,null);
}
