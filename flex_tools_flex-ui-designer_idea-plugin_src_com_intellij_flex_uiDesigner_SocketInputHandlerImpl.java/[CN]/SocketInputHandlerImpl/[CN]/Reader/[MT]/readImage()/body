{
  final int w=readUnsignedShort();
  if (w == 0) {
    return null;
  }
  final int h=readUnsignedShort();
  int l=w * h * 4;
  final byte[] data=FileUtil.loadBytes(this,l);
  for (int i=0, j=0; i < l; i+=4) {
    data[j++]=data[i + 3];
    byte r=data[i + 1];
    data[j++]=data[i + 2];
    data[j++]=r;
  }
  return new BufferedImage(COLOR_MODER,Raster.createInterleavedRaster(new DataBufferByte(data,w * h * 3),w,h,w * 3,3,new int[]{2,1,0},null),false,null);
}
