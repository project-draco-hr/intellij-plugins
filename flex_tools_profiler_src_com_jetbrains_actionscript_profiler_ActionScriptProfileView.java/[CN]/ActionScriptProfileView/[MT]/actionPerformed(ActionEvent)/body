{
  myProfilingManager.doGc(new ProfilingManager.Callback(){
    public void finished(    final String str,    IOException ex){
      if (str != null) {
        invokeOnEdt(new Runnable(){
          public void run(){
            myStatus.setText(str.substring(str.lastIndexOf(' ') + 1));
          }
        }
);
        return;
      }
      reportProblem(ex);
    }
  }
);
}
