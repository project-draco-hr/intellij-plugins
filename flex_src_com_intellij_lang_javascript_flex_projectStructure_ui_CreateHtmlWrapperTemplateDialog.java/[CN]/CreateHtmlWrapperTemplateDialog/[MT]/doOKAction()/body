{
  final VirtualFile folder=createFolderIfMissing(myProject,myWrapperFolderComponent.getComponent().getText().trim());
  if (folder != null && checkIfEmpty(myProject,folder) && createWrapper(myProject,folder,mySdk,myEnableHistoryCheckBox.isSelected(),myCheckPlayerVersionCheckBox.isSelected(),myExpressInstallCheckBox.isSelected())) {
    super.doOKAction();
  }
}
