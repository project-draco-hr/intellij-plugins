{
  ManifestHolder manifestHolder=myBundleCache.getManifestHolder(object);
  if (manifestHolder != null) {
    try {
      return manifestHolder.getBundleManifest();
    }
 catch (    ManifestHolderDisposedException e) {
      myBundleCache.markUnclean();
      return null;
    }
  }
  return null;
}
