{
  long endTimeNano=System.nanoTime();
  int afterAcceptCalls=JstdConfigFileLoader.ourAcceptCount.get();
  String message=String.format("[JsTD] Time taken by '" + name + "': %.2f ms, FileNameMatcher.acceptCalls: %d, FileNameMatcher.totalAcceptCalls: %d, extra args: %s\n",(endTimeNano - startTimeNano) / 1000000.0,afterAcceptCalls - beforeAcceptCalls,afterAcceptCalls,Arrays.toString(args));
  LOG.info(message);
}
