{
  PsiBuilder builder=createBuilder("Import-Package: com.acme;resolution:\n =optional\n");
  ASTNode node=testObject.parse(ManifestStubElementTypes.FILE,builder);
  assertThat(node.getElementType(),sameInstance((IElementType)ManifestStubElementTypes.FILE));
  checkContainsNodes(node,ManifestElementTypes.SECTION);
  node=node.getChildren(null)[0];
  checkContainsNodes(node,ManifestElementTypes.HEADER);
  node=node.getChildren(null)[0];
  checkContainsNodes(node,ManifestTokenType.HEADER_NAME,ManifestTokenType.COLON,ManifestTokenType.SIGNIFICANT_SPACE,ManifestElementTypes.CLAUSE);
  node=node.getChildren(null)[3];
  checkContainsNodes(node,ManifestElementTypes.HEADER_VALUE_PART,ManifestTokenType.SEMICOLON,ManifestElementTypes.DIRECTIVE);
  node=node.getChildren(null)[2];
  checkContainsNodes(node,ManifestElementTypes.HEADER_VALUE_PART,ManifestTokenType.COLON,ManifestTokenType.NEWLINE,ManifestTokenType.SIGNIFICANT_SPACE,ManifestTokenType.EQUALS,ManifestElementTypes.HEADER_VALUE_PART);
}
