{
  final String value=xmlAttributeValue.getValue();
  final String trimmedText=value.trim();
  final int index=xmlAttributeValue.getText().indexOf(trimmedText);
  return index < 0 || trimmedText.length() == 0 ? Pair.create(value,xmlAttributeValue.getValueTextRange()) : Pair.create(trimmedText,new TextRange(index,index + trimmedText.length()));
}
