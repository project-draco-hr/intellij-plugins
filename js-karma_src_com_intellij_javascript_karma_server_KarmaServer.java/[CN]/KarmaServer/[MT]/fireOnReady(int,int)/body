{
  if (myIsReady.compareAndSet(false,true)) {
    UIUtil.invokeLaterIfNeeded(new Runnable(){
      @Override public void run(){
        for (        KarmaServerListener listener : myListeners) {
          listener.onReady(webServerPort,runnerPort);
        }
        myOnReadyFired=true;
      }
    }
);
  }
}
