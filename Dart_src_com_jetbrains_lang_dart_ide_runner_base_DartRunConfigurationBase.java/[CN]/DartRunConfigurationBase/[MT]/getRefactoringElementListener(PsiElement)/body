{
  if (element instanceof PsiFile) {
    final String scriptFile=getFilePath();
    if (scriptFile != null) {
      VirtualFile changedFile=((PsiFile)element).getVirtualFile();
      if (changedFile != null && Comparing.equal(new File(changedFile.getPath()).getAbsolutePath(),new File(scriptFile).getAbsolutePath())) {
        return myRefactoringElementListener;
      }
    }
  }
  return null;
}
