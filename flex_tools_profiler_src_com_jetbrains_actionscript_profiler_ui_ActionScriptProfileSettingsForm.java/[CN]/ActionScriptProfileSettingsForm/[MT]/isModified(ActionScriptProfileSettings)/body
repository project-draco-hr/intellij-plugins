{
  return !Comparing.equal(settings.getHost(),myHostField.getText()) || !Comparing.equal(settings.getPort(),myPortField.getText()) || !Comparing.equal(settings.getPathToMmCfg(),FileUtil.toSystemIndependentName(myPathToMmCfgTextField.getText()))|| !Comparing.equal(settings.isUseCustomPathToMmCfg(),myUseCustomDirectoryRadioButton.isSelected());
}
