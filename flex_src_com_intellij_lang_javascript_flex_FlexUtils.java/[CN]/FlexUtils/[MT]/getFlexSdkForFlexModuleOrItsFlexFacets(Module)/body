{
  if (ApplicationManager.getApplication().isUnitTestMode()) {
    final Sdk sdk=ModuleRootManager.getInstance(module).getSdk();
    if (sdk != null && sdk.getSdkType() instanceof IFlexSdkType) {
      return sdk;
    }
  }
 else {
    return ModuleType.get(module) instanceof FlexModuleType ? createFlexSdkWrapper(FlexBuildConfigurationManager.getInstance(module).getActiveConfiguration()) : null;
  }
  return null;
}
