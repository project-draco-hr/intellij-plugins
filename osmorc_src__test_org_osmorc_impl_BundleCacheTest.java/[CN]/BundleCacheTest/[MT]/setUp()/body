{
  super.setUp();
  myCache=new BundleCache();
  myCache.updateWith(makeManifestHolder("MANIFEST.MF",bundleSymbolicName("foo.bar").bundleVersion("1.0.0").exportPackages("foo.bar")));
  myCache.updateWith(makeManifestHolder("MANIFEST2.MF",bundleSymbolicName("foo.bam").bundleVersion("1.2.0").exportPackages("foo.bam;version=1.2.0").importPackages("foo.bar")));
  myCache.updateWith(makeManifestHolder("MANIFEST3.MF",bundleSymbolicName("foo.bam").bundleVersion("1.2.3").exportPackages("foo.bam; version=1.2.3").importPackages("foo.bar")));
  myCache.updateWith(makeManifestHolder("MANIFEST4.MF",bundleSymbolicName("foo.baz").bundleVersion("1.2.1").exportPackages("foo.baz;version=1.2.0").importPackages("foo.bar","foo.bam;version=\"[1.2.3,2.0.0)\"")));
  myCache.updateWith(myFragmentHolder=makeManifestHolder("MANIFEST5.MF",bundleSymbolicName("foo.bar.naff").bundleVersion("1.0.1").exportPackages("foo.bar.naff;version=1.0.1").importPackages("foo.bar").fragmentHost("foo.bar")));
}
