{
  if (path == null || path.isEmpty()) {
    throw new ExecutionException("No flutter sdk given");
  }
  File flutterSdk=new File(path);
  File bin=new File(flutterSdk,"bin");
  if (!bin.isDirectory()) {
    throw new ExecutionException("No flutter sdk given");
  }
  if (!(new File(bin,"flutter").exists())) {
    throw new ExecutionException("No flutter sdk given");
  }
  return path;
}
