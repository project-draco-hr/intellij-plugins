{
  myLibrary=library;
  mySdkType=targetPlatform == TargetPlatform.Web ? FlexSdkType.getInstance() : targetPlatform == TargetPlatform.Desktop ? AirSdkType.getInstance() : AirMobileSdkType.getInstance();
  mySubtype=targetPlatform == TargetPlatform.Web ? Subtype.Flex : targetPlatform == TargetPlatform.Desktop ? Subtype.AIR : Subtype.AIRMobile;
}
