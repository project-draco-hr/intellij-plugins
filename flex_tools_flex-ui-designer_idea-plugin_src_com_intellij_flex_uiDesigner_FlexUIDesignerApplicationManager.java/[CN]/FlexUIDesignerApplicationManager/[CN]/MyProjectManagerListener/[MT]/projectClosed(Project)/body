{
  if (server == null || server.isClosed()) {
    return;
  }
  ProjectInfo projectInfo=project.getUserData(PROJECT_INFO);
  if (projectInfo != null) {
    project.putUserData(PROJECT_INFO,null);
    try {
      client.closeProject(project);
    }
 catch (    IOException e) {
      LOG.error(e);
    }
  }
}
