{
  boolean hasError=true;
  try {
    beginMessage(ClientMethod.closeProject);
    writeId(project);
    hasError=false;
  }
  finally {
    try {
      if (hasError) {
        blockOut.rollback();
      }
 else {
        out.flush();
      }
    }
  finally {
      unregisterProject(project);
    }
  }
}
