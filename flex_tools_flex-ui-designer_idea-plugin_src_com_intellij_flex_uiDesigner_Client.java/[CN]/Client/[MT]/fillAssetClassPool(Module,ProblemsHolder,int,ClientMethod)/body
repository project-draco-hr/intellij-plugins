{
  boolean hasError=true;
  try {
    beginMessage(method);
    writeId(module);
    out.writeShort(classCount);
    final RequiredAssetsInfo totalDefinedAssetContainerClassInfo=registeredProjects.getInfo(module.getProject()).totalDefinedAssetContainerClassInfo;
    AssetClassPoolGenerator.generate(method,classCount,totalDefinedAssetContainerClassInfo,blockOut);
    hasError=false;
  }
 catch (  Throwable e) {
    problemsHolder.add(e);
  }
 finally {
    finalizeMessage(hasError);
  }
}
