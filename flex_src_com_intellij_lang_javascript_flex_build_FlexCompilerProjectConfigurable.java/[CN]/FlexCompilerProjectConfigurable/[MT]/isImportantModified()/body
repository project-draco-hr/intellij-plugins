{
  return myConfig.USE_FCSH != myFcshRadioButton.isSelected() || myConfig.USE_MXMLC_COMPC != myMxmlcCompcRadioButton.isSelected() || myConfig.USE_BUILT_IN_COMPILER != myBuiltInCompilerRadioButton.isSelected() || myConfig.PREFER_ASC_20 != myPreferASC20CheckBox.isSelected() || !myHeapSizeTextField.getText().trim().equals(String.valueOf(myConfig.HEAP_SIZE_MB)) || !myVMOptionsEditor.getText().trim().equals(myConfig.VM_OPTIONS);
}
