{
  closeHeader();
  headerMarker=builder.mark();
  currentHeaderName=builder.getTokenText();
  builder.advanceLexer();
  while (!builder.eof() && builder.getTokenType() != ManifestTokenType.COLON && builder.getTokenType() != ManifestTokenType.NEWLINE) {
    builder.advanceLexer();
  }
  builder.advanceLexer();
}
