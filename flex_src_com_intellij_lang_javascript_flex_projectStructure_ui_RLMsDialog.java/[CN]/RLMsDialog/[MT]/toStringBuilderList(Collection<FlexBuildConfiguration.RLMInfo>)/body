{
  final List<StringBuilder> result=new ArrayList<>();
  for (  FlexBuildConfiguration.RLMInfo rlm : rlms) {
    result.add(new StringBuilder().append(rlm.MAIN_CLASS).append(CompilerOptionInfo.LIST_ENTRY_PARTS_SEPARATOR).append(rlm.OUTPUT_FILE).append(CompilerOptionInfo.LIST_ENTRY_PARTS_SEPARATOR).append(rlm.OPTIMIZE));
  }
  return result;
}
