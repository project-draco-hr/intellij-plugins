{
  doCharTest('}',"foo {{bar<caret>","foo {{bar}}<caret>");
  doCharTest('}',"foo {{&bar<caret>","foo {{&bar}}<caret>");
  doCharTest('}',"foo {{/bar<caret>","foo {{/bar}}<caret>");
  HbConfig.setAutoGenerateCloseTagEnabled(true);
  doCharTest('}',"foo {{#bar<caret>","foo {{#bar}}<caret>{{/bar}}");
  doCharTest('}',"foo {{^bar<caret>","foo {{^bar}}<caret>{{/bar}}");
  HbConfig.setAutoGenerateCloseTagEnabled(false);
  doCharTest('}',"foo {{#bar<caret>","foo {{#bar}}<caret>");
  doCharTest('}',"foo {{^bar<caret>","foo {{^bar}}<caret>");
}
