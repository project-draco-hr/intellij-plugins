{
  return new ColumnInfo<AnalysisError,AnalysisError>("Description"){
    @Nullable @Override public Comparator<AnalysisError> getComparator(){
      return myDescriptionComparator;
    }
    @Nullable @Override public TableCellRenderer getRenderer(    @NotNull final AnalysisError error){
      return MESSAGE_RENDERER;
    }
    @NotNull @Override public AnalysisError valueOf(    @NotNull final AnalysisError error){
      return error;
    }
  }
;
}
