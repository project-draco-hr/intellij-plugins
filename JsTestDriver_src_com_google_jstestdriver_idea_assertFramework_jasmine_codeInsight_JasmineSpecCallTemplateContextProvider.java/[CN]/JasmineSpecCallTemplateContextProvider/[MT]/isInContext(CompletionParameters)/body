{
  PsiElement element=parameters.getOriginalPosition();
  if (element == null) {
    return false;
  }
  final JSExpressionStatement head=JstdConfigFileUtils.getVerifiedHierarchyHead(element,new Class[]{LeafPsiElement.class,JSReferenceExpression.class},JSExpressionStatement.class);
  return head != null;
}
