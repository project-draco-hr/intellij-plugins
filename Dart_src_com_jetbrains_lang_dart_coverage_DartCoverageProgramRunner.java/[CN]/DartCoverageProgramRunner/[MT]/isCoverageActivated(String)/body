{
  try {
    ProcessOutput output=new CapturingProcessHandler(new GeneralCommandLine().withExePath(dartPubPath).withParameters("global","list")).runProcess(60 * 1000);
    return output.getExitCode() == 0 && output.getStdout().contains("coverage ");
  }
 catch (  ExecutionException e) {
    return false;
  }
}
