{
  ApplicationManager.getApplication().invokeLater(new Runnable(){
    public void run(){
      ApplicationManager.getApplication().runWriteAction(new Runnable(){
        public void run(){
          if (!module.isDisposed()) {
            doGenerateProject(module,baseDir,data);
          }
        }
      }
);
    }
  }
,ModalityState.NON_MODAL);
}
