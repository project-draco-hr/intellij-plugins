{
  if (!myFile.isValid() || myDocument == null) {
    return;
  }
  String text=myDocument.getText();
  final TokensCache tokensCache=new LexerBasedTokensCache(new MarkdownLexer(text));
  final ASTNode parsedTree=new MarkdownParser(CommonMarkMarkerProcessor.Factory.INSTANCE$).parse(MarkdownElementTypes.MARKDOWN_FILE,tokensCache);
  final String html=new HtmlGenerator(text,parsedTree).generateHtml();
  myPanel.setHtml("<html>" + html + "</html>","",new MarkdownHtmlRendererContext(myPanel));
}
