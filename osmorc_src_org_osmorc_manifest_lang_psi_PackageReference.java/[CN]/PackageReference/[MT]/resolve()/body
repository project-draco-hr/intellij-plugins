{
  final PsiPackage parentPackage=getPsiPackage();
  if (parentPackage != null) {
    for (    PsiPackage aPackage : parentPackage.getSubPackages(_moduleWithLibrariesScope)) {
      if (Comparing.equal(aPackage.getName(),getCanonicalText())) {
        return aPackage;
      }
    }
  }
  return null;
}
