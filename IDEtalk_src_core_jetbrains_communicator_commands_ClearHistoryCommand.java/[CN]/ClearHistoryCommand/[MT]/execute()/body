{
  if (myIdeFacade.askQuestion(StringUtil.getMsg("ClearHistoryCommand.title"),StringUtil.getMsg("ClearHistoryCommand.text"))) {
    try {
      UIUtil.run(myIdeFacade,StringUtil.getMsg("ClearHistoryCommand.title"),new Runnable(){
        public void run(){
          myMessageDispatcher.clearHistory();
        }
      }
);
    }
 catch (    CanceledException e) {
    }
  }
}
