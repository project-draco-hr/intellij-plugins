{
  if (!recursion_guard_(b,l,"mapTypeExpression"))   return false;
  boolean r, p;
  Marker m=enter_section_(b,l,_NONE_,null);
  r=consumeToken(b,"#@");
  p=r;
  r=r && report_error_(b,fqnTypeExpression(b,l + 1));
  r=p && consumeToken(b,"@{") && r;
  exit_section_(b,l,m,null,r,p,null);
  return r || p;
}
