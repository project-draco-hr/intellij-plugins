{
  myBCCombo=new BCCombo(myProject);
  myMainClassFilter=Conditions.alwaysFalse();
  myMainClassComponent=JSReferenceEditor.forClassName("",myProject,null,GlobalSearchScope.EMPTY_SCOPE,null,new Condition<JSClass>(){
    @Override public boolean value(    final JSClass jsClass){
      return myMainClassFilter.value(jsClass);
    }
  }
,ExecutionBundle.message("choose.main.class.dialog.title"));
}
