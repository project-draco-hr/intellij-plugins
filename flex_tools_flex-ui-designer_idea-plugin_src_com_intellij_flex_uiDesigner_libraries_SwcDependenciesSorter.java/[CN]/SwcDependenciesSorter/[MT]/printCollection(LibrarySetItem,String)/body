{
  final Writer writer=new BufferedWriter(new FileWriter(new File(appDir,library.library.getPath() + '_' + postfix+ "_unresolvedDefinitions.txt")));
  try {
    for (    CharSequence s : library.filteredDefinitions) {
      writer.append(s);
      writer.append('\n');
    }
  }
  finally {
    writer.close();
  }
}
