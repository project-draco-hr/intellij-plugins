{
  final VirtualFile sdkRoot=currentSdk.getHomeDirectory();
  if (sdkRoot == null) {
    return null;
  }
  for (  final Sdk sdk : FlexSdkUtils.getFlexSdks()) {
    if (sdkRoot.equals(sdk.getHomeDirectory()) && ArrayUtil.contains(requiredPlayerglobalSwc,sdk.getRootProvider().getFiles(OrderRootType.CLASSES))) {
      return sdk;
    }
  }
  return null;
}
