{
  if (StringUtil.compareVersionNumbers(adtVersion,"2.6") >= 0) {
    return true;
  }
  final MessageDialogWithHyperlinkListener dialog=new MessageDialogWithHyperlinkListener(module.getProject(),FlexBundle.message("air.mobile.version.problem.title"),UIUtil.getErrorIcon(),FlexBundle.message("run.air.mobile.version.problem",bc.getSdk().getName(),bc.getName(),adtVersion));
  dialog.addHyperlinkListener(new HyperlinkAdapter(){
    protected void hyperlinkActivated(    final HyperlinkEvent e){
      dialog.close(DialogWrapper.OK_EXIT_CODE);
      FlexSdkUtils.openModuleConfigurable(module);
    }
  }
);
  dialog.show();
  return false;
}
