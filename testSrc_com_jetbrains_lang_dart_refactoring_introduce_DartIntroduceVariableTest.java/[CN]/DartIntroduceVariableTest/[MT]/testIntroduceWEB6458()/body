{
  try {
    doTest();
  }
 catch (  RuntimeException e) {
    assertEquals("com.intellij.refactoring.util.CommonRefactoringUtil$RefactoringErrorHintException: Cannot perform refactoring.\n" + "Can't find place for the result",e.getMessage());
    return;
  }
  fail("Expected: Can't find place for the result");
}
