{
  try {
    final String name=FileUtil.getNameWithoutExtension(config.OUTPUT_FILE_NAME);
    CreateAirDescriptorAction.createAirDescriptor(new AirDescriptorParameters(myAirDescriptorFileName,airDescriptorFolderPath,FlexSdkUtils.getAirVersion(mySdk),FlexCompilationUtils.fixApplicationId(name),name,name,"0.0",config.OUTPUT_FILE_NAME,name,400,300,mySdk != null && mySdk.getSdkType() instanceof AirMobileSdkType));
  }
 catch (  IOException e) {
    throw new ConfigurationException("Failed to create AIR application descriptor\n" + e.getMessage());
  }
}
