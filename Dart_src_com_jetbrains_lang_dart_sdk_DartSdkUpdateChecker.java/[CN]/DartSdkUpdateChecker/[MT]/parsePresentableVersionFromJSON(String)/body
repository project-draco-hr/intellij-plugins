{
  try {
    final JSONObject obj=new JSONObject(versionJSON);
    final String version=obj.optString("version",null);
    if (version == null) {
      return null;
    }
    final String revision=obj.optString("revision",null);
    if (revision == null) {
      return version;
    }
    return version + "_r" + revision;
  }
 catch (  JSONException e) {
    throw null;
  }
}
