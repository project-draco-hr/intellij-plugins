{
  List<VmCallFrame> frames=new ArrayList<VmCallFrame>();
  for (int i=0; i < arr.length(); i++) {
    VmCallFrame frame=createFrom(isolate,arr.getJSONObject(i));
    if (i == 0 && arr.length() > 2) {
      if (DebuggerUtils.isInternalMethodName(frame.getFunctionName())) {
        continue;
      }
    }
    frames.add(frame);
  }
  return frames;
}
