{
  final DartSettings dartSettings=DartSettings.getSettings();
  if (StringUtil.isEmpty(dartSettings.getSdkPath())) {
    return getDescriptors(file,manager,isOnTheFly,DartBundle.message("inspections.dart.sdk.message"),DartBundle.message("dart.setup.sdk"));
  }
 else   if (!DartSettingsUtil.isDartSDKConfigured(file.getProject())) {
    return getDescriptors(file,manager,isOnTheFly,DartBundle.message("inspections.dart.sdk.disabled.message"),DartBundle.message("dart.enable.sdk"));
  }
  return ProblemDescriptor.EMPTY_ARRAY;
}
