{
  return new AbstractModule(){
    @Override protected void configure(){
      bind(ObjectOutput.class).annotatedWith(Names.named("testResultProtocolMessageOutput")).toInstance(myTestResultProtocolMessageOutput);
      bind(File.class).annotatedWith(Names.named("jstdConfigFile")).toInstance(configFile);
      Multibinder<TestResultListener> testResultListeners=newSetBinder(binder(),TestResultListener.class);
      testResultListeners.addBinding().to(TestResultHolder.class);
      bind(TestResultListener.class).to(MultiTestResultListener.class);
      newSetBinder(binder(),ResponseStreamFactory.class).addBinding().to(TestRunnerResponseStreamFactory.class);
    }
  }
;
}
