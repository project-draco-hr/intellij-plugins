{
  if (frameworkDir == null) {
    if (getRunConfiguration().getFrameworkDir().length() > 0) {
      String path=getRunConfiguration().getFrameworkDir();
      File dir=new File(path);
      if (!dir.exists()) {
        dir.mkdirs();
      }
      frameworkDir=dir;
    }
 else {
      frameworkDir=getWorkingDir();
    }
  }
  return frameworkDir;
}
