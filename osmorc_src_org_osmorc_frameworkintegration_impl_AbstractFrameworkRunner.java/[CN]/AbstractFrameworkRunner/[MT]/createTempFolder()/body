{
  String tempDir=PathManager.getSystemPath() + File.separator + "osmorc"+ File.separator+ "runtmp"+ System.currentTimeMillis();
  File f=new File(tempDir);
  f.mkdirs();
  try {
    _workingDirectory=f.getCanonicalPath();
  }
 catch (  IOException e) {
    throw new IllegalStateException("Could not create temp folder. " + tempDir,e);
  }
}
