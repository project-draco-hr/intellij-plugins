{
  if (SUITABLE_FILE_EXTENSIONS.contains(file.getExtension())) {
    if (SUITABLE_FILE_NAMES_WITHOUT_EXTENSION.contains(StringUtil.toLowerCase(file.getNameWithoutExtension()))) {
      return true;
    }
    if (isReferencedByJstdRunConfiguration(file)) {
      return true;
    }
  }
  return false;
}
