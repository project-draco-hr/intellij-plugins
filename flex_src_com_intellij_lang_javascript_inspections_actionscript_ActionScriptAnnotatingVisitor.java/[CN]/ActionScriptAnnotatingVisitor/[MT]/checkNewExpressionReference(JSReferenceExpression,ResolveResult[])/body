{
  PsiElement toCheck=results[0].getElement();
  if (toCheck instanceof JSFunction && ((JSFunction)toCheck).isConstructor()) {
    toCheck=toCheck.getParent();
  }
  if (toCheck instanceof JSClass && ((JSClass)toCheck).isInterface()) {
    final PsiElement referenceNameElement=referenceExpression.getReferenceNameElement();
    registerProblem(referenceNameElement,JSBundle.message("javascript.interface.can.not.be.instantiated.message"),getUnresolvedReferenceHighlightType(referenceExpression),myHolder,JSUnresolvedFunctionInspection.SHORT_NAME);
  }
}
