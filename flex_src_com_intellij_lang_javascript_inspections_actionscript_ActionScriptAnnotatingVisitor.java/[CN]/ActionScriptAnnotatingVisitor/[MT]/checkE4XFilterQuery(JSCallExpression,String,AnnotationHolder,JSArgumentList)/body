{
  if (!JSResolveUtil.isAssignableType("XML",type,argumentList) && !JSResolveUtil.isAssignableType("XMLList",type,argumentList)) {
    JSProblemUtil.registerProblem(node.getMethodExpression(),JSBundle.message("javascript.invalid.e4x.filter.query.receiver",type),ValidateTypesUtil.getHighlightTypeForTypeOrSignatureProblem(node),holder,JSValidateTypesInspection.SHORT_NAME);
    return;
  }
  ValidateTypesUtil.reportProblemIfNotOneParameter(node,holder);
  JSExpression[] arguments=argumentList.getArguments();
  if (arguments.length >= 1) {
    ValidateTypesUtil.checkExpressionIsAssignableToType(arguments[0],BOOLEAN_CLASS_NAME,holder,argumentList.getContainingFile(),"javascript.argument.type.mismatch",null);
  }
}
