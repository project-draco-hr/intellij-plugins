{
  if (url == null) {
    return null;
  }
  URI uri=URI.create(url);
  if (uri.getScheme() == null) {
    return url;
  }
  if (url.startsWith(ECLIPSE_FORMAT)) {
    url=uri.getScheme() + "://" + uri.getPath();
  }
 else {
    url=uri.getScheme() + ":" + uri.getSchemeSpecificPart();
  }
  return url;
}
