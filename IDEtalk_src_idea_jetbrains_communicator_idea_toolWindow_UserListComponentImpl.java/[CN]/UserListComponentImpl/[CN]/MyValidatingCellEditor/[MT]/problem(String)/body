{
  new Thread(new Runnable(){
    public void run(){
      try {
        Thread.sleep(100);
      }
 catch (      InterruptedException e) {
      }
      UIUtil.invokeLater(new Runnable(){
        public void run(){
          Callout.showText(myTree,myTree.getEditingPath(),Callout.SOUTH_WEST,StringUtil.getMsg(resourceCode));
        }
      }
);
    }
  }
).start();
  return false;
}
