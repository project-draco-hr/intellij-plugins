{
  List<State> states=nameToState.get(name);
  if (states == null) {
    states=new ArrayList<State>(5);
    states.add(state);
    nameToState.put(name,states);
  }
 else {
    if (states.contains(state)) {
      MxmlWriter.LOG.warn("State " + state + " already added to map for "+ name);
    }
 else {
      states.add(state);
    }
  }
}
