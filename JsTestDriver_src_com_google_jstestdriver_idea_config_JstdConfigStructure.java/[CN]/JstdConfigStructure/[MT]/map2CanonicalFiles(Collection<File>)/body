{
  Set<File> canonicalFiles=Sets.newHashSet();
  for (  File file : files) {
    try {
      canonicalFiles.add(file.getCanonicalFile());
    }
 catch (    IOException e) {
      LOG.warn("Can't find canonical file for '" + file.getAbsolutePath() + "'... Just skipping.",e);
    }
  }
  return canonicalFiles;
}
