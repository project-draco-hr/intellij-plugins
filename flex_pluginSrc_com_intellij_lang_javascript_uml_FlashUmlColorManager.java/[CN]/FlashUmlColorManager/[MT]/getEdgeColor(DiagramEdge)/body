{
  return edge == null || !isInterface(FlashUmlDataModel.getIdentifyingElement(edge.getSource())) || !isInterface(FlashUmlDataModel.getIdentifyingElement(edge.getTarget())) || edge.getRelationship().getStartArrow() != DiagramRelationshipInfo.DELTA || edge.getRelationship().getLineType() != DiagramLineType.SOLID ? super.getEdgeColor(edge) : REALIZATION;
}
