{
  FindHbScriptHtmlLexer lexer=new FindHbScriptHtmlLexer();
  try {
    String text=VfsUtilCore.loadText(file);
    lexer.start(text);
    while (lexer.getTokenType() != null) {
      lexer.advance();
      if (lexer.isSeenHbScriptTag()) {
        return true;
      }
    }
  }
 catch (  IOException e) {
    return false;
  }
  return false;
}
