{
  int low=0;
  int high=regions.size() - 1;
  while (low <= high) {
    int mid=(low + high) >>> 1;
    PluginNavigationRegion midVal=regions.get(mid);
    int cmp=midVal.getOffset() - offset;
    if (cmp < 0) {
      low=mid + 1;
    }
 else     if (cmp > 0) {
      high=mid - 1;
    }
 else {
      return midVal;
    }
  }
  return null;
}
