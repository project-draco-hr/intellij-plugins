{
  if (location1 == null || location2 == null) {
    return false;
  }
  int line1=getLineNumberFromLocation(location1.getIsolate(),location1);
  int line2=getLineNumberFromLocation(location2.getIsolate(),location2);
  if (line1 <= 0 || line2 <= 0) {
    return false;
  }
  return line1 == line2;
}
