{
  myElement=element;
  String value=getValue(myElement);
  int length=value != null ? value.length() : 0;
  if (length == 0) {
    myStart=0;
    myEnd=0;
  }
 else   if (element instanceof CssString || FlexCssUtil.inQuotes(myElement.getText())) {
    myStart=1;
    myEnd=length + 1;
  }
 else {
    myStart=0;
    myEnd=length;
  }
}
