{
  if (!new File(getTempKeystorePath()).exists()) {
    final CertificateParameters params=new CertificateParameters(new File(getTempKeystorePath()).getPath(),TEMP_CERTIFICATE_NAME,TEMP_KEY_TYPE,TEMP_KEYSTORE_PASSWORD,"","","");
    return PackageAirInstallerAction.createCertificate(project,flexSdk,params);
  }
  return true;
}
