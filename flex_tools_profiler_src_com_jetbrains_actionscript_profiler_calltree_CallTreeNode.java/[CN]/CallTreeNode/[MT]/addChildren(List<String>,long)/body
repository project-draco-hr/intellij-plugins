{
  CallTreeNode node=this;
  for (  String frame : reversedFrames) {
    frame=stripCallDelims(frame);
    CallTreeNode child=node.findChildByName(frame);
    if (child == null) {
      child=new CallTreeNode(frame,0);
      node.addChild(child);
    }
    child.duration+=duration;
    node=child;
  }
}
