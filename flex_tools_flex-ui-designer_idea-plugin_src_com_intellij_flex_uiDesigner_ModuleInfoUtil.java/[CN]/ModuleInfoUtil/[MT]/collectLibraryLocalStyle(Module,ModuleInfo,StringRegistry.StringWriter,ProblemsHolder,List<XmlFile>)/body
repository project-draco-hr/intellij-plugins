{
  VirtualFile defaultsCss=null;
  for (  VirtualFile sourceRoot : ModuleRootManager.getInstance(moduleInfo.getModule()).getSourceRoots(false)) {
    if ((defaultsCss=sourceRoot.findChild(Library.DEFAULTS_CSS)) != null) {
      break;
    }
  }
  if (defaultsCss != null) {
    moduleInfo.addLocalStyleHolder(new LocalStyleHolder(defaultsCss,new LocalCssWriter(stringWriter,problemsHolder,unregisteredDocumentReferences).write(defaultsCss,module)));
  }
}
