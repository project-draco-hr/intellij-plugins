{
  final AccessToken token=ReadAction.start();
  try {
    final JSClass jsClass=XmlBackedJSClassImpl.getXmlBackedClass(psiFile);
    return jsClass != null && (JSInheritanceUtil.isParentClass(jsClass,SPARK_APPLICATION) || JSInheritanceUtil.isParentClass(jsClass,MX_APPLICATION));
  }
  finally {
    token.finish();
  }
}
