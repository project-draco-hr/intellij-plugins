{
  final PsiFile psiFile=CommonDataKeys.PSI_FILE.getData(e.getDataContext());
  final boolean available=psiFile instanceof XmlFile && DartSettingsUtil.getDartiumPath() != null && isHtmlFileWithDartScript(((XmlFile)psiFile));
  e.getPresentation().setEnabled(available);
  e.getPresentation().setVisible(available);
}
