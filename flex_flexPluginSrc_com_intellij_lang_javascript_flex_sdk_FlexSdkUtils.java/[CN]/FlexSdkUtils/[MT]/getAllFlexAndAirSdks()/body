{
  List<Sdk> result=new ArrayList<Sdk>();
  final Sdk[] sdks=ProjectJdkTable.getInstance().getAllJdks();
  for (  Sdk sdk : sdks) {
    final SdkType sdkType=sdk.getSdkType();
    if (sdkType instanceof FlexSdkType || sdkType instanceof AirSdkType) {
      result.add(sdk);
    }
  }
  return result;
}
