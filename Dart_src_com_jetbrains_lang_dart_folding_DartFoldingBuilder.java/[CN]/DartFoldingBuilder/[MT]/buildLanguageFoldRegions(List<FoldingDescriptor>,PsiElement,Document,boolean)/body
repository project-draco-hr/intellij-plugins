{
  if (!(root instanceof DartFile))   return;
  final TextRange fileHeaderRange=foldFileHeader(descriptors,(DartFile)root,document);
  foldImportExportStatements(descriptors,(DartFile)root);
  foldComments(descriptors,root,fileHeaderRange);
  foldClassBodies(descriptors,(DartFile)root);
  foldFunctionBodies(descriptors,root);
}
