{
  final String functionName=cssFunction.getFunctionName();
  @SuppressWarnings("ConstantConditions") final ASTNode valueNode=cssFunction.getNode().findChildByType(CssElementTypes.CSS_TERM_LIST).getFirstChildNode().getFirstChildNode();
switch (functionName.charAt(0)) {
case 'C':
    writeClassReference(info,valueNode);
  break;
case 'E':
writeEmbed(cssFunction,valueNode);
break;
case 'P':
writePropertyReference(valueNode);
default :
throw new IllegalArgumentException("unknown function: " + functionName);
}
}
