  m_faultTable.add(ScriptTimeoutFault.name);
  m_faultTable.putDescription(InvalidTargetFault.name,getLocalizationManager().getLocalizedTextString("invalidTargetFault"));
  m_faultTable.putDescription(RecursionLimitFault.name,getLocalizationManager().getLocalizedTextString("recursionLimitFault"));
  m_faultTable.putDescription(InvalidWithFault.name,getLocalizationManager().getLocalizedTextString("invalidWithFault"));
  m_faultTable.putDescription(ProtoLimitFault.name,getLocalizationManager().getLocalizedTextString("protoLimitFault"));
  m_faultTable.putDescription(InvalidURLFault.name,getLocalizationManager().getLocalizedTextString("invalidUrlFault"));
  m_faultTable.putDescription(ExceptionFault.name,getLocalizationManager().getLocalizedTextString("exceptionFault"));
  m_faultTable.putDescription(StackUnderFlowFault.name,getLocalizationManager().getLocalizedTextString("stackUnderflowFault"));
  m_faultTable.putDescription(DivideByZeroFault.name,getLocalizationManager().getLocalizedTextString("divideByZeroFault"));
  m_faultTable.putDescription(ScriptTimeoutFault.name,getLocalizationManager().getLocalizedTextString("scriptTimeoutFault"));
  m_faultTable.action(InvalidTargetFault.name,"stop");
  m_faultTable.action(InvalidTargetFault.name,"print");
  m_faultTable.action(RecursionLimitFault.name,"stop");
  m_faultTable.action(RecursionLimitFault.name,"print");
  m_faultTable.action(InvalidWithFault.name,"stop");
  m_faultTable.action(InvalidWithFault.name,"print");
  m_faultTable.action(ProtoLimitFault.name,"stop");
  m_faultTable.action(ProtoLimitFault.name,"print");
  m_faultTable.action(InvalidURLFault.name,"stop");
  m_faultTable.action(InvalidURLFault.name,"print");
  m_faultTable.action(ExceptionFault.name,"stop");
  m_faultTable.action(ExceptionFault.name,"print");
  m_faultTable.action(StackUnderFlowFault.name,"stop");
  m_faultTable.action(StackUnderFlowFault.name,"print");
  m_faultTable.action(DivideByZeroFault.name,"stop");
  m_faultTable.action(DivideByZeroFault.name,"print");
