{
  return new Consumer<List<String>>(){
    public void consume(    final List<String> messages){
      final String reason=StringUtil.join(messages,"<br>");
      NOTIFICATION_GROUP.createNotification("",FlexBundle.message("failed.to.create.air.package",PathUtil.getFileName(packagePath),reason),NotificationType.ERROR,null).notify(project);
    }
  }
;
}
