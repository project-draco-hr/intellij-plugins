{
  return new JavaElementVisitor(){
    public void visitReferenceExpression(    PsiReferenceExpression expression){
    }
    @Override public void visitClass(    PsiClass psiClass){
      if (OsmorcFacet.hasOsmorcFacet(psiClass)) {
        PsiJavaFile file=(PsiJavaFile)psiClass.getContainingFile();
        if ("".equals(file.getPackageName())) {
          holder.registerProblem(psiClass.getNameIdentifier(),"Class is in default package",ProblemHighlightType.GENERIC_ERROR_OR_WARNING);
        }
      }
    }
  }
;
}
