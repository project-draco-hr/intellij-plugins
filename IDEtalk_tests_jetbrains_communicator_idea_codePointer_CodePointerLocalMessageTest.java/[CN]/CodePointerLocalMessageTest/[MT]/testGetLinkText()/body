{
  testLinkText("a file (2..4)","a file",1,3);
  testLinkText("file (2..4)","a path/file",1,3);
  testLinkText("file (2..4)","/a path/file",1,3);
  testLinkText("file (2..4)","a path\\file",1,3);
  testLinkText("file (2..4)","\\a path\\file",1,3);
  testLinkText("file (2..4)","/a path\\file",1,3);
  testLinkText("file (2..4)","\\a path/file",1,3);
  testLinkText("file:4","file",3,3);
}
