{
  List<VirtualFile> copiedFiles=Lists.newArrayList();
  for (  VirtualFile virtualFile : virtualFiles) {
    try {
      copiedFiles.add(VfsUtil.copyFile(null,virtualFile,targetDir));
    }
 catch (    IOException e) {
      Messages.showErrorDialog("Extract operation failed!\nUnable to copy " + virtualFile.getPath() + " to "+ targetDir.getPath(),"Adding " + myAssertionFrameworkName + " adapter support for JsTestDriver");
      return null;
    }
  }
  return copiedFiles;
}
