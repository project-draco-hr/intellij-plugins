{
  BufferedWriter out=null;
  BufferedReader in=null;
  try {
    out=new BufferedWriter(new FileWriter(from));
    in=new BufferedReader(new FileReader(to));
    int ch;
    while ((ch=in.read()) != -1) {
      out.write(ch);
    }
  }
 catch (  IOException e) {
  }
 finally {
    if (out != null) {
      try {
        out.close();
      }
 catch (      IOException e) {
      }
    }
    if (in != null) {
      try {
        in.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
