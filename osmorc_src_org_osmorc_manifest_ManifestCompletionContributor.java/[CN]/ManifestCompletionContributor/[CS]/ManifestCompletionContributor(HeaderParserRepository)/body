{
  final Set<String> availableHeaders=headerParserRepository.getAllHeaderNames();
  extend(CompletionType.BASIC,PlatformPatterns.psiElement(ManifestTokenTypes.HEADER_NAME).withLanguage(ManifestLanguage.INSTANCE),new CompletionProvider<CompletionParameters>(){
    public void addCompletions(    @NotNull CompletionParameters completionparameters,    ProcessingContext processingcontext,    @NotNull CompletionResultSet completionresultset){
      LookupElementFactoryImpl lookupelementfactoryimpl=LookupElementFactoryImpl.getInstance();
      for (      String availableHeader : availableHeaders) {
        completionresultset.addElement(lookupelementfactoryimpl.createLookupElement(availableHeader));
      }
    }
  }
);
}
