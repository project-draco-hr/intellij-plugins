{
  install(new HtmlDocModule());
  install(new DebugModule(true));
  Injector injector=Guice.createInjector(new CompositeModule(modules),new JsTestDriverModule(flags,resolvedConfiguration.getFilesList(),resolvedConfiguration.getServer(flags.getServer(),flags.getPort(),flags.getServerHandlerPrefix()),System.out,resolvedConfiguration.getBasePath(),resolvedConfiguration.getTestSuiteTimeout(),resolvedConfiguration.getTests(),Collections.<FileInfo>emptyList(),resolvedConfiguration.getProxyConfiguration()));
  return injector.getInstance(ActionRunner.class);
}
