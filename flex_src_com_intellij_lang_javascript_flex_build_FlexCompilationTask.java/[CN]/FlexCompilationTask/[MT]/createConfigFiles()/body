{
  if (myOldConfig != null) {
    return FlexCompilationUtils.getConfigFiles(myOldConfig,myModule,myFlexFacet,null);
  }
 else {
    final Sdk sdk=FlexUtils.getFlexSdkForFlexModuleOrItsFlexFacets(myModule);
    return Collections.singletonList(CompilerConfigGenerator.getOrCreateConfigFile(myModule,sdk.getHomePath(),sdk.getVersionString(),myFlexIdeConfig));
  }
}
