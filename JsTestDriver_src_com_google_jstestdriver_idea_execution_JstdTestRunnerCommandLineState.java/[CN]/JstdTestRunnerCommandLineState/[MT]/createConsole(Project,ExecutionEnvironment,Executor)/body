{
  JstdRunConfiguration runConfiguration=(JstdRunConfiguration)env.getRunProfile();
  TestConsoleProperties testConsoleProperties=new SMTRunnerConsoleProperties(new RuntimeConfigurationProducer.DelegatingRuntimeConfiguration<JstdRunConfiguration>(runConfiguration),JSTD_FRAMEWORK_NAME,executor);
  testConsoleProperties.setUsePredefinedMessageFilter(false);
  testConsoleProperties.setIfUndefined(TestConsoleProperties.HIDE_PASSED_TESTS,false);
  SMTRunnerConsoleView smtConsoleView=SMTestRunnerConnectionUtil.createConsoleWithCustomLocator(JSTD_FRAMEWORK_NAME,testConsoleProperties,env,new JstdTestLocationProvider(),true,new JstdTestProxyFilterProvider(testConsoleProperties.getProject()));
  Disposer.register(project,smtConsoleView);
  return smtConsoleView;
}
