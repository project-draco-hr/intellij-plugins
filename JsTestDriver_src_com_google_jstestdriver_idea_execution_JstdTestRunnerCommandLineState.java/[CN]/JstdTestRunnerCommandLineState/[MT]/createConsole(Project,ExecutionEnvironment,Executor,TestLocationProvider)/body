{
  JstdRunConfiguration runConfiguration=(JstdRunConfiguration)env.getRunProfile();
  TestConsoleProperties testConsoleProperties=new SMTRunnerConsoleProperties(new RuntimeConfigurationProducer.DelegatingRuntimeConfiguration<JstdRunConfiguration>(runConfiguration),JSTD_FRAMEWORK_NAME,executor);
  ConsoleView testsOutputConsoleView=SMTestRunnerConnectionUtil.createConsoleWithCustomLocator(JSTD_FRAMEWORK_NAME,testConsoleProperties,env.getRunnerSettings(),env.getConfigurationSettings(),locationProvider,true);
  Disposer.register(project,testsOutputConsoleView);
  return testsOutputConsoleView;
}
