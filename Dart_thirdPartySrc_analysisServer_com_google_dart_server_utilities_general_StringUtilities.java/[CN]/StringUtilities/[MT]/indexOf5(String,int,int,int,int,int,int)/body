{
  int index=startIndex;
  int last=string.length() - 4;
  while (index < last) {
    if (string.charAt(index) == char1 && string.charAt(index + 1) == char2 && string.charAt(index + 2) == char3 && string.charAt(index + 3) == char4 && string.charAt(index + 4) == char5) {
      return index;
    }
    index++;
  }
  return -1;
}
