{
  String executable=s.getExecutable();
  PhoneGapUIUtil.setExecutablePath(myExecutablePathField,!StringUtil.isEmpty(executable) ? executable : PhoneGapSettings.getInstance().getExecutablePath());
  String item=getCommandsMap().get(s.getPlatform());
  if (item != null) {
    myPlatformField.setSelectedItem(item);
  }
}
