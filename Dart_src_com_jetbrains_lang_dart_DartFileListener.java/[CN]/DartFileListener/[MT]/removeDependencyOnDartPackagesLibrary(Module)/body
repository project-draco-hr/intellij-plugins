{
  final ModifiableRootModel modifiableModel=ModuleRootManager.getInstance(module).getModifiableModel();
  try {
    for (    final OrderEntry orderEntry : modifiableModel.getOrderEntries()) {
      if (orderEntry instanceof LibraryOrderEntry && LibraryTablesRegistrar.PROJECT_LEVEL.equals(((LibraryOrderEntry)orderEntry).getLibraryLevel()) && DART_PACKAGES_LIBRARY_NAME.equals(((LibraryOrderEntry)orderEntry).getLibraryName())) {
        modifiableModel.removeOrderEntry(orderEntry);
      }
    }
    if (modifiableModel.isChanged()) {
      ApplicationManager.getApplication().runWriteAction(new Runnable(){
        @Override public void run(){
          modifiableModel.commit();
        }
      }
);
    }
  }
  finally {
    if (!modifiableModel.isDisposed()) {
      modifiableModel.dispose();
    }
  }
}
