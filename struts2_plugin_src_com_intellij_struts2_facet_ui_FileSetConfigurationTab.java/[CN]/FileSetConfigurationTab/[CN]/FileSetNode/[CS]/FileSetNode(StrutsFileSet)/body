{
  mySet=fileSet;
  final PresentationData presentationData=getPresentation();
  final String name=mySet.getName();
  if (fileSet.getFiles().isEmpty()) {
    presentationData.addText(name,getErrorAttributes());
    presentationData.setTooltip(StrutsBundle.message("facet.fileset.nofiles.attached"));
  }
 else {
    presentationData.addText(name,getPlainAttributes());
    presentationData.addText(" (" + fileSet.getFiles().size() + ")",SimpleTextAttributes.GRAY_ITALIC_ATTRIBUTES);
  }
  if (fileSet.isAutodetected()) {
    presentationData.addText(StrutsBundle.message("facet.fileset.autodetected"),SimpleTextAttributes.GRAY_ITALIC_ATTRIBUTES);
  }
}
