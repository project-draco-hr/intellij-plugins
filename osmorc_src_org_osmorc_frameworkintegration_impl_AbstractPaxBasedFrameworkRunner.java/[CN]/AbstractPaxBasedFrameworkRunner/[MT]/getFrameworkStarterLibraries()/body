{
  @SuppressWarnings({"ConstantConditions"}) final String paxLib=PluginManager.getPlugin(PluginId.getId("Osmorc")).getPath().getPath() + "/lib/pax-runner-1.3.0.jar";
  List<VirtualFile> libs=new ArrayList<VirtualFile>(1);
  libs.add(LocalFileSystem.getInstance().findFileByPath(paxLib));
  return libs;
}
