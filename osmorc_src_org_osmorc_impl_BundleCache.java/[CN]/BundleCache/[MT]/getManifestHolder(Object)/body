{
  for (  ManifestHolder manifestHolder : myManifestHolders) {
    Object boundObject;
    try {
      boundObject=manifestHolder.getBoundObject();
    }
 catch (    ManifestHolderDisposedException ignore) {
      continue;
    }
    if (boundObject.equals(bundle)) {
      return manifestHolder;
    }
  }
  return null;
}
