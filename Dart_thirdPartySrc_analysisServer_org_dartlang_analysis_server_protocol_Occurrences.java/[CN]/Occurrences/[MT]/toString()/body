{
  StringBuilder builder=new StringBuilder();
  builder.append("[");
  builder.append("element=");
  builder.append(element + ", ");
  builder.append("offsets=");
  builder.append(StringUtils.join(offsets,", ") + ", ");
  builder.append("length=");
  builder.append(length);
  builder.append("]");
  return builder.toString();
}
