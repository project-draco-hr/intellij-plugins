{
  final FacetDetectorRegistryEx<StrutsFacetConfiguration> registry=(FacetDetectorRegistryEx<StrutsFacetConfiguration>)facetDetectorRegistry;
  registry.registerUniversalDetectorByFileNameAndRootTag(StrutsConstants.STRUTS_XML_DEFAULT_FILENAME,StrutsRoot.TAG_NAME,new StrutsFacetDetector(),WebUtilImpl.BY_PARENT_WEB_ROOT_SELECTOR);
}
