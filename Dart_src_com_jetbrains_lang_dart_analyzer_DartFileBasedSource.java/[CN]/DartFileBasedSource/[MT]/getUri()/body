{
  String path=myFile.getPath();
  if (!path.startsWith("/")) {
    path="/" + path;
  }
  try {
    return new URI(myFile.getFileSystem().getProtocol(),null,path,null);
  }
 catch (  URISyntaxException e) {
    Logger.getInstance(DartFileBasedSource.class).error(myFile.getUrl(),e);
  }
  return null;
}
