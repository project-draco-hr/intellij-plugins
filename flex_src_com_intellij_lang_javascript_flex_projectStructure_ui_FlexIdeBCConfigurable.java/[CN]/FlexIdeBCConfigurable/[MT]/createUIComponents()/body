{
  rebuildMainClassFilter();
  myMainClassComponent=JSReferenceEditor.forClassName("",myModule.getProject(),null,GlobalSearchScope.moduleScope(myModule),null,new Condition<JSClass>(){
    @Override public boolean value(    final JSClass jsClass){
      return myMainClassFilter.value(jsClass);
    }
  }
,ExecutionBundle.message("choose.main.class.dialog.title"));
}
