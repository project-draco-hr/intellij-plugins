{
  final String baseClass=myConfiguration.getOutputType() == OutputType.RuntimeLoadedModule ? FlexCompilerSettingsEditor.MODULE_BASE_CLASS_NAME : FlexCompilerSettingsEditor.SPRITE_CLASS_NAME;
  final Condition<JSClass> filter=new JSClassChooserDialog.PublicInheritor(myModule,baseClass,true);
  myMainClassComponent=JSReferenceEditor.forClassName("",myModule.getProject(),null,GlobalSearchScope.moduleScope(myModule),null,filter,ExecutionBundle.message("choose.main.class.dialog.title"));
}
