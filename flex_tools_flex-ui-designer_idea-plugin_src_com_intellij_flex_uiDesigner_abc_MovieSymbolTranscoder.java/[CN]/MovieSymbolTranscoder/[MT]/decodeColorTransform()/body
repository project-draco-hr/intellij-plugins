{
  syncBits();
  boolean hasAdd=readBit();
  boolean hasMult=readBit();
  int nbits=readUBits(4);
  if (hasMult) {
    readSBits(nbits);
    readSBits(nbits);
    readSBits(nbits);
    readSBits(nbits);
  }
  if (hasAdd) {
    readSBits(nbits);
    readSBits(nbits);
    readSBits(nbits);
    readSBits(nbits);
  }
}
