{
  super("JSTestDriver:ConfigurationType",MessageBundle.getPluginName(),MessageBundle.getPluginName(),JsTestDriverIcons.JsTestDriver);
  addFactory(new ConfigurationFactory(this){
    @Override public RunConfiguration createTemplateConfiguration(    Project project){
      return new JstdRunConfiguration(project,this,MessageBundle.getPluginName());
    }
  }
);
}
