{
  super("JSTestDriver:ConfigurationType",PluginResources.getPluginName(),PluginResources.getPluginName(),PluginResources.getJstdSmallIcon());
  addFactory(new ConfigurationFactory(this){
    @Override public RunConfiguration createTemplateConfiguration(    Project project){
      return new JstdRunConfiguration(project,this,PluginResources.getPluginName());
    }
  }
);
}
