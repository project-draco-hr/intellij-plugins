{
  PsiElement resolved=resolveUniquely(jsReferenceExpression);
  JSVariable jsVariable=CastUtils.tryCast(resolved,JSVariable.class);
  if (jsVariable != null) {
    JSExpression initializer=jsVariable.getInitializer();
    if (initializer != null) {
      return initializer;
    }
  }
  return null;
}
