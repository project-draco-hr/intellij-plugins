{
  final int pathStartIndex=text.indexOf(prefix);
  if (pathStartIndex < 0 || pathStartIndex > 0 && !isCharAllowedBeforeOrAfterPath(text.charAt(pathStartIndex - 1))) {
    return null;
  }
  final int dotDartIndex=text.toLowerCase().indexOf(".dart",pathStartIndex);
  final int pathEndIndex=dotDartIndex + ".dart".length();
  if (dotDartIndex <= 0 || text.length() > pathEndIndex && !isCharAllowedBeforeOrAfterPath(text.charAt(pathEndIndex))) {
    return null;
  }
  return Pair.create(pathStartIndex,pathEndIndex);
}
