{
  String[] s=text.split(Pattern.quote("_"));
  int[] data=new int[s.length];
  for (int i=0; i < s.length; i++) {
    data[i]=Integer.parseInt(s[i]);
    if (data[i] < 0) {
      throw new RuntimeException("All components of CompoundId should be greater than 0, " + text);
    }
  }
  myData=data;
}
